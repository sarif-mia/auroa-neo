{%- liquid
  assign options = section.settings

  assign img_before = options.before_img
  assign img_after = options.after_img
  assign desktop_image_ratio = options.desktop_image_ratio | times: 100
  assign mobile_image_ratio = options.mobile_image_ratio | times: 100
  assign subheading = options.subheading
  assign heading = options.heading
  assign heading_size = options.heading_size
  assign body_richtext = options.body_richtext
  assign body_size = options.body_size
  assign btn_url = options.btn_url
  assign btn_style = options.btn_style
  assign btn_text = options.btn_text
  assign content_alignment = options.content_alignment
  assign content_paddings_size = options.content_paddings_size
  assign add_desktop_side_paddings = options.add_desktop_side_paddings
  assign add_mobile_side_paddings = options.add_mobile_side_paddings

  assign container_size = options.container_size
  assign round_level = options.round_level
  assign image_position = options.layout

  assign before_img_label = options.before_img_label
  assign before_img_subheading = options.before_img_subheading
  assign before_img_heading = options.before_img_heading
  assign before_img_heading_size = options.before_img_heading_size
  assign before_img_heading_color = options.before_img_heading_color

  assign after_img_label = options.after_img_label
  assign after_img_subheading = options.after_img_subheading
  assign after_img_heading = options.after_img_heading
  assign after_img_heading_size = options.after_img_heading_size
  assign after_img_heading_color = options.after_img_heading_color

  assign drag_position = options.drag

  assign color_scheme = settings.default_color_scheme
  if options.color_scheme_variant == 'own'
    assign color_scheme = options.color_scheme
  endif

  assign disable_lazy_loading = options.disable_lazy_loading

  assign enable_content_slide_in = settings.enable_content_slide_in
  assign enable_image_zoom_out = settings.enable_image_zoom_out

  assign image_class = 'before-after__image'
  if enable_image_zoom_out
    assign image_class = image_class | append: ' scroll-trigger animate--zoom-out'
  endif
-%}

{%- liquid
  assign show_background = section.settings.show_background
  assign color_container_background = false

  if show_background == false and settings.default_color_scheme.settings.background != color_scheme.settings.background
    assign color_container_background = true
  endif
-%}

{% capture css %}
  {% style %}
    .section-{{ section.id}}{
      --gsc-section-spacing-top: var(--gsc-padding-block-{{ section.settings.section_spacing_top }});
      --gsc-section-spacing-bottom: var(--gsc-padding-block-{{ section.settings.section_spacing_bottom }});
      --gsc-before-image-heading-color: {{ before_img_heading_color }};
      --gsc-after-image-heading-color: {{ after_img_heading_color }};
      --gsc-img-ratio: {{ mobile_image_ratio }}%;
      {% if image_position != 'bottom' %}
        --gsc-content-inline-paddings: var(--gsc-content-padding-{{ content_paddings_size }});
      {% endif %}
    }

    @media {% render 'media-queries', screen: 'md' %} {
      .section-{{ section.id}} {
        --gsc-img-ratio: {{ desktop_image_ratio }}%;
      }
    }
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

<div class='section {% if round_level != 'default' %}round-level-{{round_level}}{% endif %} section-{{ section.id }} color-{{ color_scheme }} before-after spacing-block {% if add_mobile_side_paddings %}spacing-inline-mobile{% endif %} {% if add_desktop_side_paddings %}spacing-inline-desktop{% endif %} {% if color_container_background == false %}section-background{% endif %}'>
  <div class='container container--{{ container_size }} before-after__container {% if color_container_background %}container-background{% endif %} before-after__container--with-image-on-{{ image_position }}-side'>
    {% if subheading != blank or heading != blank or body_richtext != blank %}
      <div
        class='before-after__content before-after__content--with-{{ content_alignment }}-alignment {% if image_position != 'bottom' %} before-after__content--constrained {% endif %}'
      >
        {% if subheading != blank %}
          <span
            class='caption body-color {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
            {% if enable_content_slide_in %}
              style='--gsc-animation-order: 1;'
            {% endif %}
          >
            {{ subheading }}
          </span>
        {% endif %}

        {% if heading != blank %}
          <h2
            class='{{ heading_size }} before-after__title {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
            {% if enable_content_slide_in %}
              style='--gsc-animation-order: 2;'
            {% endif %}
          >
            {{ heading }}
          </h2>
        {% endif %}

        {% if body_richtext != blank %}
          <div
            class='{{ body_size }} {% if heading != blank %} body-color {% endif %} body-font-weight-from-global-settings before-after__body {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
            {% if enable_content_slide_in %}
              style='--gsc-animation-order: 3;'
            {% endif %}
          >
            {{ body_richtext }}
          </div>
        {% endif %}

        {%- if btn_text != blank -%}
          {%- if btn_style == 'link' -%}
            <a
              class='link before-after__btn {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
              {% if enable_content_slide_in %}
                style='--gsc-animation-order: 4;'
              {% endif %}
              {% if btn_url != blank %}
                href='{{ btn_url }}'
              {% endif %}
              aria-label='{{ btn_text | strip_html | escape }}'
            >
              <span class='link-parent-for-hover'>
                <span class='link__text'>{{ btn_text }}</span>
              </span>
              {%- render 'icon', icon_name: 'link-chevron', class: 'link__chevron' -%}
            </a>
          {%- else -%}
            <a
              class='btn btn--{{ btn_style }} before-after__btn {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
              {% if enable_content_slide_in %}
                style='--gsc-animation-order: 4;'
              {% endif %}
              {% if btn_url != blank %}
                href='{{ btn_url }}'
              {% endif %}
              aria-label='{{- btn_text | strip_html | escape -}}'
            >
              {{- btn_text -}}
            </a>
          {%- endif -%}
        {%- endif -%}
      </div>
    {% endif %}

    <div class='before-after__images-wrapper'>
      <div class='shape shape--nature'>
        <before-after-images
          class='before-after__images shape__target-image before-after--with-transition'
          data-initial-drag-position='{{drag_position}}'
        >
          <div class='before-after__before'>
            {%- if img_before != blank -%}
              <img
                {% render 'image-attributes',
                  image: img_before,
                  class: image_class,
                  is_eager_loading: disable_lazy_loading
                %}
              >
            {%- else -%}
              {{ 'image' | placeholder_svg_tag: 'image placeholder-image' }}
            {%- endif -%}

            {% if before_img_label != blank %}
              <div class='before-after__tag before-after__tag--left'>
                {{ before_img_label }}
              </div>
            {% endif %}

            {% if before_img_subheading != blank or before_img_heading != blank %}
              <div class='before-after__caption before-after__caption--left'>
                {% if before_img_subheading != blank %}
                  <p class='before-after__img-subheading'>{{ before_img_subheading }}</p>
                {% endif %}

                {% if before_img_heading != blank %}
                  <p class='before-after__img-heading {{ before_img_heading_size }}'>{{ before_img_heading }}</p>
                {% endif %}
              </div>
            {% endif %}
          </div>

          <div class='before-after__after'>
            {%- if img_after != blank -%}
              <img
                {% render 'image-attributes',
                  image: img_after,
                  class: image_class,
                  is_eager_loading: disable_lazy_loading
                %}
              >
            {%- else -%}
              {{ 'image' | placeholder_svg_tag: 'image placeholder-image' }}
            {%- endif -%}

            {% if after_img_label != blank %}
              <div class='before-after__tag before-after__tag--right'>
                {{ after_img_label }}
              </div>
            {% endif %}

            {% if after_img_subheading != blank or after_img_heading != blank %}
              <div class='before-after__caption before-after__caption--right'>
                {% if after_img_subheading != blank %}
                  <p class='before-after__img-subheading'>{{ after_img_subheading }}</p>
                {% endif %}

                {% if after_img_heading != blank %}
                  <p class='before-after__img-heading {{ after_img_heading_size }}'>{{ after_img_heading }}</p>
                {% endif %}
              </div>
            {% endif %}
          </div>

          <div class='before-after__separator' data-before-after-images-separator>
            <span class='before-after__separator-line'></span>
            <button
              class='before-after__separator-button'
              type='button'
              aria-label='{{ 'before_after_images.screen_reader.separator.button' | t }}'
              data-before-after-images-button
            >
              {% render 'icon', icon_name: 'before-after-separator' %}
            </button>
          </div>
        </before-after-images>
      </div>
    </div>
  </div>
</div>

