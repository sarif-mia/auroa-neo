{%- liquid
  assign options = section.settings

  assign color_scheme = settings.default_color_scheme
  if options.color_scheme_variant == 'own'
    assign color_scheme = options.color_scheme
  endif
  assign round_level = options.round_level
  assign add_desktop_side_paddings = options.add_desktop_side_paddings
  assign add_mobile_side_paddings = options.add_mobile_side_paddings
  assign section_spacing_top = options.section_spacing_top
  assign section_spacing_bottom = options.section_spacing_bottom
  assign title = options.title
  assign text = options.text
  assign container_size = options.container_size
  assign body_size = options.body_size
  assign content_alignment = options.content_alignment
  assign heading_size = options.heading_size
  assign btn_url = options.btn_url
  assign btn_style = options.btn_style
  assign btn_text = options.btn_text
  assign section_layout = options.section_layout
  assign color_tabs = options.color_tabs

  assign not_empty_blocks_indexes = ''

  for item in section.blocks
    if item.settings.title != blank
      case item.type
        when 'category'
          assign index = forloop.index0
          if forloop.index0 != section.blocks.size
            assign index = index | append: '///'
          endif

          assign not_empty_blocks_indexes = not_empty_blocks_indexes | append: index

        when 'question'
          if item.settings.image != blank or item.settings.video != blank or item.settings.page != blank or item.settings.text != blank
            assign index = forloop.index0
            if forloop.index0 != section.blocks.size
              assign index = index | append: '///'
            endif

            assign not_empty_blocks_indexes = not_empty_blocks_indexes | append: index
          endif
      endcase
    endif
  endfor

  assign not_empty_blocks_indexes = not_empty_blocks_indexes | split: '///'

  assign first_question = blank

  for index in not_empty_blocks_indexes
    assign index_num = index | times: 1
    if section.blocks[index_num].type == 'question'
      assign first_question = section.blocks[index_num]
      break
    endif
  endfor

  assign enable_content_slide_in = settings.enable_content_slide_in
-%}

{%- liquid
  assign show_background = section.settings.show_background
  assign color_container_background = false

  if show_background == false and settings.default_color_scheme.settings.background != color_scheme.settings.background
    assign color_container_background = true
  endif
-%}

{% capture css %}
  {% style %}
    .section-{{ section.id }} {
      --gsc-section-spacing-top: var(--gsc-padding-block-{{ section_spacing_top }});
      --gsc-section-spacing-bottom: var(--gsc-padding-block-{{ section_spacing_bottom }});
    }

    {% for block in section.blocks %}
      .block-{{ block.id }} {
        --gsc-image-width: {{ block.settings.image_width }}%;
        --gsc-icon-size: {{ block.settings.icon_size }}px;
      }
    {% endfor %}
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

{% if not_empty_blocks_indexes.size > 0 %}
  <accordeon-container class='faq {% if round_level != 'default' %}round-level-{{round_level}}{% endif %} section-{{ section.id }} faq--{{ section_layout }} {% if title == blank and text == blank and btn_text == blank %} faq--without-content {% endif %} section color-{{ color_scheme }} faq--with-background spacing-block {% if add_mobile_side_paddings %}spacing-inline-mobile{% endif %} {% if add_desktop_side_paddings %}spacing-inline-desktop{% endif %} {% if color_container_background == false %}section-background{% endif %}'>
    <div class='container container--{{ container_size }} {% if color_container_background %}container-background{% endif %}'>
      <div class='faq__container  {% if section_layout == 'left-tabs' %} faq__container--with-tabs-on-left-side {% elsif section_layout == 'right-tabs' %} faq__container--with-tabs-on-right-side {% elsif section_layout == 'bottom-tabs' %} faq__container--with-tabs-on-bottom-side {% endif %}'>
        {%- if title != blank or text != blank or btn_text != blank -%}
          <div class='faq__content {% if content_alignment != 'left' %}faq__content--align-{{content_alignment}}{% endif %} {% if container_size == 'full-screen' and settings.round_level != "0" %}{% if section_layout == 'left-tabs' %} faq__content--with-right-spacing {% elsif section_layout == 'right-tabs' %} faq__content--with-left-spacing {% endif %}{% endif %}'>
            <div class='faq__content-body {% if color_container_background == false and add_desktop_side_paddings == false or add_mobile_side_paddings == false  %}spacing-inline-header{% endif %}'>
              {%- if title != blank -%}
                <h2
                  class='{{ heading_size }} {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                  {% if enable_content_slide_in %}
                    style='--gsc-animation-order: 1;'
                  {% endif %}
                >
                  {{ title }}
                </h2>
              {%- endif -%}
              {%- if text != blank -%}
                <div
                  class='rte {{ body_size }} {% if title != blank %} body-color {% endif %} faq__text body-font-weight-from-global-settings {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                  {% if enable_content_slide_in %}
                    style='--gsc-animation-order: 2;'
                  {% endif %}
                >
                  {{ text }}
                </div>
              {%- endif -%}

              {%- if btn_text != blank -%}
                {%- if btn_style == 'link' -%}
                  <a
                    class='link faq__btn {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                    {% if enable_content_slide_in %}
                      style='--gsc-animation-order: 3;'
                    {% endif %}
                    {% if btn_url != blank %}
                      href='{{ btn_url }}'
                    {% endif %}
                    aria-label='{{ btn_text | strip_html | escape }}'
                  >
                    <span class='link-parent-for-hover'>
                      <span class='link__text'>{{ btn_text }}</span>
                    </span>
                    {%- render 'icon', icon_name: 'link-chevron', class: 'link__chevron' -%}
                  </a>
                {%- else -%}
                  <a
                    class='btn btn--{{ btn_style }} faq__btn {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                    {% if enable_content_slide_in %}
                      style='--gsc-animation-order: 4;'
                    {% endif %}
                    {% if btn_url != blank %}
                      href='{{ btn_url }}'
                    {% endif %}
                    aria-label='{{- btn_text | strip_html | escape -}}'
                  >
                    {{- btn_text -}}
                  </a>
                {%- endif -%}
              {%- endif -%}
            </div>
          </div>
        {%- endif -%}

        {%- if section.blocks.size > 0 -%}
          <div class='faq__blocks {% if color_tabs == 'all' %} faq__blocks--with-background faq__blocks--with-aside-spacing {% endif %} {% if color_tabs == 'none' %} faq__blocks--full-width {% endif %} {% if color_tabs contains 'separately' %} faq__blocks--with-separately-devided {% endif %}'>
            {%- for index in not_empty_blocks_indexes -%}
              {% liquid
                assign index_num = index | times: 1
                assign block = section.blocks[index_num]
                assign icon_type = block.settings.icon_type
                assign icon = block.settings.icon
                assign custom_icon_image = block.settings.custom_icon_image
              %}

              {%- case block.type -%}
                {%- when 'category' -%}
                  <div
                    class='faq__category block-{{ block.id }} {{ block.settings.category_heading_size }} {% if color_tabs == 'all' %} faq__category--all-without-aside-spacing {% endif %} {% if color_tabs == 'content_tabs_only' %} faq__category--without-aside-spacing {% endif %} {% if color_tabs == 'categories_only' or color_tabs == 'all_separately' %} faq__category--with-background {% endif %} {% if color_tabs == 'content_tabs_separately' %} faq__category--without-aside-spacing {% endif %} {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                    {% if enable_content_slide_in %}
                      style='--gsc-animation-order: {{forloop.index}};'
                    {% endif %}
                    {{ block.shopify_attributes }}
                  >
                    {%- if icon_type == 'custom' and custom_icon_image != blank -%}
                      <img
                        {% render 'image-attributes',
                          image: custom_icon_image,
                          size: 'small',
                          class: 'faq__category-icon'
                        %}
                      >
                    {%- elsif icon_type == 'preset' -%}
                      {% render 'icon-pack', icon: icon, class: 'faq__category-icon' %}
                    {%- endif -%}

                    {{ block.settings.title }}
                  </div>

                {%- when 'question' -%}
                  {%- liquid
                    assign media_type = block.settings.media_type
                    assign image = block.settings.image
                    assign video = block.settings.video
                    assign page = block.settings.page
                    assign prevIndex = forloop.index0 | minus: 1
                    assign nextIndex = forloop.index0 | plus: 1
                    assign isFirst = false
                    assign isLast = false

                    if section.blocks[prevIndex].type == 'category' or section.blocks[prevIndex].type == blank or forloop.index0 == 0
                      assign isFirst = true
                    endif

                    if section.blocks[nextIndex].type == 'category' or section.blocks[nextIndex].type == blank
                      assign isLast = true
                    endif
                  -%}

                  <div
                    class='faq__question faq__question--outer-container {% if color_tabs == 'all' %} faq__question--without-aside-spacing {% endif %} {% if color_tabs == 'categories_only' %} faq__question--full-width {% endif %} {% if color_tabs == 'content_tabs_only' %} faq__question--with-background {% endif %} {% if color_tabs == 'content_tabs_only' and isFirst %} faq__question--first {% endif %} {% if color_tabs == 'content_tabs_only' and  isLast %} faq__question--last {% endif %} {% if color_tabs == 'content_tabs_only' and isLast and isFirst %} faq__question--first-and-last {% endif %} {% if color_tabs contains 'separately' %} faq__question--with-separately-background {% endif %} {% if enable_content_slide_in %}scroll-trigger animate--slide-in{% endif %}'
                    {% if enable_content_slide_in %}
                      style='--gsc-animation-order: {{forloop.index}};'
                    {% endif %}
                  >
                    <accordeon-component
                      class='faq__question faq__accordeon-plus accordeon-plus block-{{ block.id }} {% if color_tabs == 'none' or color_tabs == 'all' or isFirst != true %} accordeon-plus--top-bordered {% endif %}'
                      data-aria-expanded='{% if section.settings.expand_first_tab and block.id == first_question.id %}true{% else %}false{% endif %}'
                      data-section-id='{{ section.id }}'
                      data-block-id='{{ block.id }}'
                      {{ block.shopify_attributes }}
                    >
                      <accordeon-button
                        class='faq__accordeon-plus-btn accordeon-plus__btn {{ block.settings.heading_size }} {% if color_tabs == 'categories_only' %} faq__accordeon-plus-btn--full-width {% endif %}'
                        tabindex='0'
                      >
                        {%- if icon_type == 'custom' and custom_icon_image != blank -%}
                          <img
                            {% render 'image-attributes',
                              image: custom_icon_image,
                              size: 'small',
                              class: 'accordeon-plus__btn-icon'
                            %}
                          >
                        {%- elsif icon_type == 'preset' -%}
                          {% render 'icon-pack', icon: icon, class: 'accordeon-plus__btn-icon' %}
                        {%- endif -%}

                        {{ block.settings.title }}

                        {% render 'icon', icon_name: 'plus', class: 'accordeon-plus__icon' %}
                      </accordeon-button>
                      <div class='accordeon-plus__content-wrap' data-accordeon-content-wrap>
                        <div
                          class='faq__accordeon-plus-content body-color accordeon-plus__content body-font-weight-from-global-settings {% if color_tabs == 'categories_only' %} faq__accordeon-plus-content--full-width {% endif %}'
                          data-accordeon-content
                        >
                          {% if block.settings.text != blank %}
                            <div class='rte {{ block.settings.body_size }}'>
                              {{ block.settings.text }}
                            </div>
                          {% endif %}
                          {% if page != blank %}
                            <div class='rte rte--page {{ block.settings.body_size }}'>
                              {{ page.content }}
                            </div>
                          {% endif %}
                          {%- if video != blank and media_type == 'video' -%}
                            {% render 'video-with-alt',
                              video: video,
                              class: 'faq__question-image',
                              autoplay: true,
                              loop: true,
                              muted: true,
                              controls: false,
                              loading: 'lazy',
                              iterate_on_click: true
                            %}
                          {%- elsif image != blank and media_type == 'image' -%}
                            <img
                              {% render 'image-attributes', image: image, class: 'faq__question-image' %}
                            >
                          {%- endif -%}
                        </div>
                      </div>
                    </accordeon-component>
                  </div>
              {%- endcase -%}
            {%- endfor -%}
          </div>
        {%- endif -%}
      </div>
    </div>
  </accordeon-container>
{% endif %}

{% render 'save-section-data-in-global-scope', section: section %}

