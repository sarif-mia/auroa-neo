{%- liquid
  for block in section.blocks
    case block.type
      when 'age_verify'
        render 'popup-age-verifier', block: block
      when 'promo_lb'
        render 'popup-promo', block: block, popup_variant: 'lightbox'
      when 'promo_flyout'
        render 'popup-promo', block: block, popup_variant: 'flyout'
      when 'signup_lb'
        render 'popup-signup', block: block, popup_variant: 'lightbox'
      when 'signup_flyout'
        render 'popup-signup', block: block, popup_variant: 'flyout'
    endcase
  endfor
-%}

