{%- liquid
  assign options = section.settings
  assign color_scheme = settings.default_color_scheme
  if options.color_scheme_variant == 'own'
    assign color_scheme = options.color_scheme
  endif
  assign round_level = options.round_level
  assign desktop_logo_img = options.desktop_logo_img
  assign desktop_logo_width = options.desktop_logo_width
  assign mobile_logo_img = options.mobile_logo_img
  assign mobile_logo_width = options.mobile_logo_width
  assign account_mobile_position = options.account_mobile_position
  assign menu = options.menu
  assign menu_type = options.menu_type
  assign transparent_visibility = options.transparent_visibility
  assign is_transparent_only_on_homepage = options.is_transparent_only_on_homepage
  assign desktop_transparent_logo_img = options.desktop_transparent_logo_img
  assign mobile_transparent_logo_img = options.mobile_transparent_logo_img
  assign transparent_opacity = options.transparent_opacity | divided_by: 100.00
  assign transparent_blurring = options.transparent_blurring | divided_by: 2.00
  assign transparent_background_color = options.transparent_background_color | color_modify: 'alpha', transparent_opacity
  assign transparent_background_gradient = options.transparent_background_gradient

  if transparent_background_gradient != blank
    assign transparent_background_color = transparent_background_gradient
  endif

  assign transparent_items_color = options.transparent_items_color
  assign show_language_selector = options.show_language_selector
  assign show_currency_selector = options.show_currency_selector
  assign show_currency_flags = options.show_currency_flags

  assign show_icons_as = options.show_icons_as
  assign cart_icon = options.cart_icon
  assign icon_style = options.icon_style
  assign icon_stroke_width = options.icon_stroke_width
  assign burger_icon_stroke_width = icon_stroke_width
  if icon_stroke_width == 'solid'
    assign burger_icon_stroke_width = 1.5
  endif
  assign show_account = options.show_account
  assign icon_variant = cart_icon | append: '-icon-with-styles'

  if show_icons_as == 'labels_only'
    assign icon_class = 'header__icon--desktop-hidden'
  endif

  assign enable_sticky = options.enable_sticky
  assign enable_sticky_mobile = options.enable_sticky_mobile
  assign enable_border = options.enable_border
  assign enable_shadow = options.enable_shadow
  assign profile_url = routes.account_login_url
  assign container_size = options.container_size
  assign menu_trigger = 'hover'
  if options.open_menu_on_click
    assign menu_trigger = 'click'
  endif

  assign enable_color_menu_items = options.enable_color_menu_items
  assign color_menu_items = options.color_menu_items | split: ', ' | split: ','

  if customer
    assign profile_url = routes.account_url
  endif

  assign add_desktop_side_paddings = options.add_desktop_side_paddings
  assign section_spacing_block = options.section_spacing_block
  assign section_spacing_inline = options.section_spacing_inline
  assign mobile_section_spacing_block = options.mobile_section_spacing_block

  if is_transparent_only_on_homepage == true and request.page_type != 'index'
    assign transparent_visibility = 'none'
  endif

  assign menu_links_size = menu.links.size
-%}

{% liquid
  assign menu_type = options.menu_type
  assign menu_position = options.menu_position
  assign logo_alignment = options.logo_alignment

  assign grid = options.grid

  if menu_type == 'drawer'
    assign grid_class = 'logo-' | append: logo_alignment
  else
    assign grid_class = 'grid--' | append: 'logo-' | append: logo_alignment | append: '-menu-' | append: menu_position
  endif
%}

{%- liquid
  assign show_background = section.settings.show_background
  assign section_background_color = color_scheme.settings.background

  assign color_container_background = true

  if show_background
    assign color_container_background = false
  endif

  if settings.default_color_scheme.settings.background == color_scheme.settings.background
    assign color_container_background = false
  endif

  if options.color_scheme_variant == 'default' and options.container_size != 'full-screen'
    assign color_container_background = true
  endif
-%}

{%- capture css -%}
  {% style %}
    .shopify-section-header {
      {% if enable_sticky_mobile != 'none' %}
        position: sticky;
        top: var(--gsc-sticky-bar-bottom, 0px);
      {% endif %}

      {% if color_container_background == false %}
        background: {{ section_background_color }};
      {% endif %}

      --gsc-header-padding-block: var(--gsc-padding-block-{{ mobile_section_spacing_block }});
      --gsc-header-padding-inline: var(--gsc-padding-inline-{{ section_spacing_inline }});

      --gsc-logo-width: {{mobile_logo_width}}px;
    }

    @media {% render 'media-queries', screen: 'md' %} {
      .shopify-section-header {
        --gsc-header-padding-block: var(--gsc-padding-block-{{ section_spacing_block }});
        --gsc-logo-width: {{desktop_logo_width}}px;
        
        {% if enable_sticky != 'none' %}
          position: sticky;
          {% if color_container_background and settings.round_level != '0' and round_level != '0' %}
            top: calc(var(--gsc-sticky-bar-bottom, 20px));
          {% else %}
            top: var(--gsc-sticky-bar-bottom, 0px);
          {% endif %}
        {% else %}
          position: relative;
          top: unset;
        {% endif %}
        
        {% if add_desktop_side_paddings %}
          padding-inline: var(--gsc-aside-padding-size, 0);
        {% endif %}
      }

      
      {% if color_container_background == true %}
        .header {
          position: relative;
        }
      {% endif %}
    }
    
    {% if transparent_visibility != "none" %}
      .shopify-section-header:has([is-transparent]) {
        {% if enable_sticky_mobile != 'none' %}
          position: sticky;
          top: var(--gsc-sticky-bar-bottom, 0px);
        {% else %}
          position: static;
        {% endif %}

        width: 100%;
        
        --gsc-transparent-opacity: {{ transparent_opacity }};
        --gsc-transparent-background: {{ transparent_background_color }};
        --gsc-transparent-items-color: {{ transparent_items_color }};
        --gsc-transparent-blur: {{ transparent_blurring }}px;
        
        {% if color_container_background == false %}
          {% if transparent_visibility contains 'mobile' %}
            background: var(--gsc-transparent-background);
            backdrop-filter: blur(var(--gsc-transparent-blur));
          {% endif %}
        {% endif %}
      }
       
      {% if color_container_background == false %}
        {% if transparent_background_gradient != blank %}            
          .shopify-section-header:has([is-transparent])::after {
            content: '';
            position: absolute;
            inset: 0;
            background: {{ section_background_color }};

            opacity: 0;
            transition: opacity 0.20s ease-in-out 0.05s;
          }

          .shopify-section-header:has([is-transparent]):has(.hamburger.is-opened)::after {
            opacity: 1;
          }
        {% endif %}    

        .shopify-section-header:has([is-transparent]):has(.hamburger.is-opened) {
          {% if transparent_background_gradient == blank %}   
            background: {{ section_background_color }};
          {% endif %}
          color: var(--gsc-text-color-100);
          border-color: var(--gsc-border-color-100);
        }
        .shopify-section-header.scrolled[with-sticky-colored]:has([is-transparent]) {
          background: {{ section_background_color }};
        }
      {% endif %}

      @media {% render 'media-queries', screen: 'md' %} {
        {% if color_container_background == false %}
          .shopify-section-header:has([is-transparent]) {
            {% if transparent_visibility contains 'desktop' %}
              background: var(--gsc-transparent-background);
              backdrop-filter: blur(var(--gsc-transparent-blur));
            {% else %}
              background: {{ section_background_color }};
            {% endif %}
          }
        {% endif %}

        .shopify-section-header:not([dynamic-sticky]):has([is-transparent]) {
          {% if enable_sticky != 'none' %}
            position: sticky;
          {% else %}
            position: static;
          {% endif %}

          {% if color_container_background and settings.round_level != '0' and round_level != '0' %}
            top: calc(var(--gsc-sticky-bar-bottom, 0px) + min(var(--gsc-aside-padding-size, 0px), 24px));
          {% else %}
            top: var(--gsc-sticky-bar-bottom, 0px);
          {% endif %}
        }
        
        {% if color_container_background == false %}
          {% if transparent_background_gradient != blank %} 
            .shopify-section-header:has([is-transparent]):hover::after {
              opacity: 1;
            }
          {% endif %}  

          .shopify-section-header:has([is-transparent]):hover {
            {% if transparent_background_gradient == blank %}   
              background: {{ section_background_color }};
            {% endif %}    
            color: var(--gsc-text-color-100);
            border-color: var(--gsc-border-color-100);
          }
          
          .shopify-section-header:has([is-transparent]):hover .header__localization-settings,
          .shopify-section-header:has([is-transparent]):hover .header__logo,
          .shopify-section-header:has([is-transparent]):hover .header__item,
          .shopify-section-header:has([is-transparent]):hover .header__icon {
            color: var(--gsc-text-color-100);
            transition: color 0.25s ease-in-out 0.05s, opacity 0.25s ease-in-out 0.05s;
          }
        {% endif %}     
      }
    {% endif %}    


    {% if enable_color_menu_items %}
      {% for link in menu.links %}
        {% assign link_title_downcase = link.title | downcase %}      

          {% for color in color_menu_items %}
            {% assign title_color_downcase = color | downcase | replace: '"' %}

            {% if title_color_downcase contains link_title_downcase %}
              {% assign title_color = title_color_downcase | split: ':' | last | replace: '[' | replace: ']' %}
              [menu-title='{{link.title}}'] {
                color: {{title_color}};
                --gsc-text-color-100: {{title_color}};
                --gsc-primary-button-bg-color-100: {{title_color}};
              }
              {% break %}
            {% endif %}
          {% endfor %}
        {% endfor %} 
    {% endif %}

    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'menu-1' %}
          {%- liquid
            assign options = block.settings
            assign mobile_img_ratio = options.mobile_img_ratio | times: 100
            assign desktop_img_ratio = options.desktop_img_ratio | times: 100
            assign space_between_cards = options.space_between_cards
            assign space_between_cards_mobile = options.space_between_cards_mobile
            assign content_position = options.content_position
            assign mobile_position = options.mobile_position
            assign header_block_paddings_desktop = options.header_block_paddings_desktop
            assign header_block_paddings_mobile = options.header_block_paddings_mobile
          
            assign color_scheme_variant = options.color_scheme_variant
            assign current_overlay_color_scheme = color_scheme

            if color_scheme_variant == 'own'
              assign current_overlay_color_scheme = options.color_scheme
            endif

            assign custom_overlay_color = current_overlay_color_scheme.settings.background.rgb | replace: ' ', ', '
            assign custom_text_color = current_overlay_color_scheme.settings.text

            if color_scheme_variant == 'custom'
              assign custom_overlay_color = options.custom_overlay_color.rgb | replace: ' ', ', '
              assign custom_text_color = options.custom_text_color
            endif
          -%}

          .mega-menu-{{block.id}} {
            {% if options.enable_overlay %}
              --gsc-overlay-color-rgb: {{ custom_overlay_color }};
              --gsc-overlay-opacity: {{ options.overlay_opacity |  divided_by: 100.00 }};              
            {% endif %}
  
            {% if color_scheme_variant == 'custom' %}
              --gsc-primary-button-text-color: {{ options.custom_button_text_color }};
              --gsc-primary-button-bg-color: {{ options.custom_button_bg_color }};
              --gsc-primary-button-bg-color-rgb: {{ options.custom_button_bg_color.red }}, {{ options.custom_button_bg_color.green }}, {{ options.custom_button_bg_color.blue }};
              --gsc-primary-button-bg-hover: {% if options.custom_button_bg_color.lightness < 50 %}{{ options.custom_button_bg_color | color_lighten: 8 }}{% else %}{{ options.custom_button_bg_color | color_darken: 8 }}{% endif %};
              --gsc-primary-button-border-color: {% if options.custom_button_border_color.alpha > 0 %}{{ options.custom_button_border_color }}{% else %}transparent{% endif %};

              --gsc-link-button-text-color: {{ options.custom_link_color }};
              --gsc-image-block-text-color: {{ custom_text_color }};    
            {% endif %}      
          }          

          .drawer-menu__page .mega-menu-{{block.id}} {
            --gsc-columns: 1;
            --gsc-img-ratio: {{ mobile_img_ratio }}%;
            --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards_mobile }});  
            --gsc-mega-menu-block-paddings: {{header_block_paddings_mobile}}px;      
          }          
              
          .drawer-menu__page .mega-menu-{{block.id}} .image-block__content-wrap,
          .drawer-menu__page .mega-menu-{{block.id}} .image-block--show-text-on-hover .image-block__text-wrapper,
          .drawer-menu__page .mega-menu-{{block.id}} .image-block__content {       
            {% render 'position-and-alignment', content_position: mobile_position, is_vertical: options.enable_vertical_text %}
          }
          
          .drawer-menu__page .image-block__content-wrap {
            padding: var(--gsc-content-padding-2xs);
          }

          .drawer-menu__page .collection-card__content.collection-card__content--on-image {
            padding: var(--gsc-content-padding-2xs);
          }

          {% if options.enable_vertical_text == true %}
            .mega-menu-{{block.id}} .image-block__content-wrap {
              text-align: unset;
            }
            
            .drawer-menu__page .mega-menu-{{block.id}} .image-block__content {
              writing-mode: vertical-rl;
              transform: scale(-1, -1);
            }

            .mega-menu-{{block.id}} .image-block__content > * + * {
              margin: 0 1.2rem 0 0;
              padding: 0;
            }

            .mega-menu-{{block.id}} .btn {
              padding: 3.2rem 1.2rem;
            }
          {% endif %}

          @media {% render 'media-queries', screen: 'md' %} {
            .mega-menu-{{block.id}}{
              --gsc-columns: {{options.columns}};
              --gsc-img-ratio: {{ desktop_img_ratio }}%;
              --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards }});  
              --gsc-mega-menu-block-paddings: {{header_block_paddings_desktop}}px;   
            }            

            .mega-menu-{{block.id}} .image-block--show-text-on-hover .image-block__text-wrapper, 
            .mega-menu-{{block.id}} .image-block__content {
              {% render 'position-and-alignment', content_position: content_position, is_vertical: options.enable_vertical_text %}
            }

            .mega-menu-{{block.id}} .image-block__content-wrap {
              {% render 'position-and-alignment', content_position: content_position, is_vertical: options.enable_vertical_text %}
              padding: var(--gsc-content-padding-2xs);
            }

            .mega-menu__block--collection-list .collection-card__content.collection-card__content--on-image {
              padding: var(--gsc-content-padding-2xs);
            }

            {% if options.enable_vertical_text == true %}            
              .header .mega-menu-{{block.id}}.mega-menu__block--vertical .image-block__content {
                {% render 'position-and-alignment', content_position: content_position, is_vertical: options.enable_vertical_text %}
                  writing-mode: vertical-rl;
                  transform: scale(-1, -1);
                }
            {% endif %}
          }
          
        {% when 'menu-2' %}
          {%- liquid
            assign options = block.settings
            assign columns = options.columns
            assign columns_float = columns | times: 1.0
            assign slide_width = 100 | divided_by: columns_float
            assign space_between_cards = options.space_between_cards
            assign space_between_cards_mobile = options.space_between_cards_mobile
            assign header_block_paddings_desktop = options.header_block_paddings_desktop
            assign header_block_paddings_mobile = options.header_block_paddings_mobile
          -%}

          .block-{{block.id}}{
            --gsc-slide-width: {{slide_width}}%;
            --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards_mobile }});
            --gsc-mega-menu-block-paddings: {{ header_block_paddings_mobile }}px;
          }

          @media {% render 'media-queries', screen: 'md' %} {
            .block-{{block.id}} {
              --gsc-mega-menu-block-paddings: {{ header_block_paddings_desktop }}px;
              --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards }});  
            }
            .drawer-menu__page .block-{{block.id}} {
              --gsc-slide-width: 82%;
              --gsc-mega-menu-block-paddings: {{ header_block_paddings_mobile }}px;
              --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards_mobile }});  
            }
          }
        {% when 'menu-3' %}
          {%- liquid
              assign options = block.settings
              assign columns = options.columns
              assign columns_float = columns | times: 1.0
              assign slide_width = 100 | divided_by: columns_float
              assign space_between_cards = options.space_between_cards
              assign space_between_cards_mobile = options.space_between_cards_mobile
              assign header_block_paddings_desktop = options.header_block_paddings_desktop
              assign header_block_paddings_mobile = options.header_block_paddings_mobile
          -%}
          {%- liquid
            assign cards_color_scheme_variant = block.settings.cards_color_scheme_variant
            assign cards_current_color_scheme = section.settings.color_scheme

            if cards_color_scheme_variant == 'own'
              assign cards_current_color_scheme = block.settings.cards_color_scheme
            endif

            assign overlay_color = cards_current_color_scheme.settings.background.rgb | replace: ' ', ', '
            assign title_color = cards_current_color_scheme.settings.text

            if cards_color_scheme_variant == 'custom'
              assign overlay_color = block.settings.cards_overlay_color.rgb | replace: ' ', ', '
              assign title_color = block.settings.cards_title_color
            endif

            assign cards_enable_overlay = false
            if block.settings.text_style contains 'on-image'
              assign cards_enable_overlay = true
            endif
          -%}

          .block-{{block.id}} {
              --gsc-columns: 1;
              --gsc-slide-width: {{ slide_width }}%;
              --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards_mobile }});  
              --gsc-mega-menu-block-paddings: {{ header_block_paddings_mobile }}px;

              --gsc-collection-card-title-color: {{ title_color }};
              {% if cards_enable_overlay %}
                --gsc-collection-card-overlay-color: {{ overlay_color  }};
                --gsc-collection-card-overlay-opacity: {{ block.settings.cards_overlay_opacity |  divided_by: 100.00 }};
              {% endif %}
          }

          @media {% render 'media-queries', screen: 'md' %} {
              .block-{{block.id}} {
                --gsc-columns: {{columns}};
                --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards }});
                --gsc-mega-menu-block-paddings: {{ header_block_paddings_desktop }}px;
              }
              .drawer-menu__page .block-{{block.id}} {
                --gsc-columns: 1;
                --gsc-slide-width: 82%;
                --gsc-space-between-cards: var(--gsc-gap-{{ space_between_cards_mobile }});
                --gsc-mega-menu-block-paddings: {{ header_block_paddings_mobile }}px;
              }
          }
      {% endcase %}
    {% endfor %}
  {% endstyle %}
{%- endcapture -%}

{%- render 'inline-css-minifier', css: css -%}

{% capture search_icon %}
  {% if settings.search_type != 'hide' %}
    {%- if settings.search_type == 'page_only' or template.name contains 'search' -%}
      <a
        class='icon header__icon unstyled-link'
        href='{{ routes.search_url }}'
        aria-label='{{ 'header.screen_reader.aria_label_for_search_icon' | t }}'
      >
        {% if show_icons_as contains 'labels' %}
          <p class='header__icon-label'>{{ 'header.store_front.label_for_search_icon' | t }}</p>
        {% endif %}

        {% render 'buy-icons-pack', icon: 'search-icon-with-styles', class: icon_class, icon_style: icon_style, stroke_width: icon_stroke_width %}
      </a>
    {%- else -%}
      <sidebar-button>
        <button
          class='unstyled-button icon header__icon'
          data-sidebar-button
          data-sidebar-id='SearchSidebar'
          aria-label='{{ 'header.screen_reader.aria_label_for_search_icon' | t }}'
        >
          {% if show_icons_as contains 'labels' %}
            <p class='header__icon-label'>{{ 'header.store_front.label_for_search_icon' | t }}</p>
          {% endif %}

          {% render 'buy-icons-pack', icon: 'search-icon-with-styles', class: icon_class, icon_style: icon_style, stroke_width: icon_stroke_width %}
        </button>
      </sidebar-button>
    {%- endif -%}
  {% endif %}
{% endcapture %}

{% capture profile_icon %}
  {%- if show_account and profile_url != blank -%}
    <a
      class='icon header__icon header__icon--people unstyled-link'
      href='{{ profile_url }}'
      tabindex='0'
      aria-label='{{ 'header.screen_reader.aria_label_for_account_icon' | t }}'
    >
      {% if show_icons_as contains 'labels' %}
        <p class='header__icon-label'>
          {% if customer %}
            {{ 'header.store_front.label_for_account_icon' | t }}
          {% else %}
            {{ 'header.store_front.label_for_login_icon' | t }}
          {% endif %}</p>
      {% endif %}

      {% render 'buy-icons-pack', icon: 'account-icon-with-styles', class: icon_class, icon_style: icon_style, stroke_width: icon_stroke_width %}
    </a>
  {%- endif -%}  
{% endcapture %}

{% capture cart_count_icon %}
  <cart-count id='CartCount'>
    {%- if cart != empty -%}
      <div class='cart-count {% if cart.item_count > 9 %}cart-count--ellipse{% endif %}'>
        <span aria-hidden='true'>{{ cart.item_count }}</span>
      </div>
    {%- endif -%}
  </cart-count>
{% endcapture %}

{% capture cart_icon_variant %}
  {% if show_icons_as contains 'labels' %}
    <p class='header__icon-label'>{{ 'header.store_front.label_for_cart_icon' | t }}</p>
  {% endif %}

  <div class='header__cart-icon'>
    {% render 'buy-icons-pack', icon: icon_variant, class: icon_class, icon_style: icon_style, stroke_width: icon_stroke_width %}
    {{cart_count_icon}}    
  </div>
{% endcapture %}

{% capture sticky_cart_icon_variant %}
  <div class='header__cart-icon'>
    {% render 'buy-icons-pack', icon: icon_variant, class: 'sticky-cart-button__icon', icon_style: icon_style, stroke_width: icon_stroke_width %}
    {{cart_count_icon}}
  </div>
{% endcapture %}

{% capture logos %}
  {% liquid
    if mobile_transparent_logo_img == blank and mobile_logo_img != blank
      assign mobile_transparent_logo_img = desktop_transparent_logo_img
    endif

    if mobile_transparent_logo_img == blank and desktop_transparent_logo_img != blank
      assign mobile_transparent_logo_img = desktop_transparent_logo_img
    endif  

    if mobile_transparent_logo_img == blank and desktop_logo_img != blank
      assign mobile_transparent_logo_img = desktop_logo_img
    endif

    if desktop_transparent_logo_img == blank and desktop_logo_img != blank
      assign desktop_transparent_logo_img = desktop_logo_img
    endif    
    
    if mobile_logo_img == blank and desktop_logo_img != blank
      assign mobile_logo_img = desktop_logo_img
    endif   

    assign logo_class_mobile = 'header__logo header__logo--mobile'
    assign logo_class_desktop = 'header__logo header__logo--desktop'

    if transparent_visibility != 'none'
      if transparent_visibility contains 'mobile' 
        assign logo_class_mobile = 'header__logo header__logo--initial header__logo--mobile'
        assign logo_class_mobile_transparent = 'header__logo header__logo--transparent header__logo--mobile'
      endif

      if transparent_visibility contains 'desktop'
        assign logo_class_desktop = 'header__logo header__logo--initial header__logo--desktop'
        assign logo_class_desktop_transparent = 'header__logo header__logo--transparent header__logo--desktop'
      endif
    endif
  %}

  {%- render 'logo',
    class: logo_class_mobile,
    image: mobile_logo_img,
    logo_width: mobile_logo_width,
    logo_text: shop.name, 
    is_in_header: true
  -%}

  {%- render 'logo',
    class: logo_class_desktop,
    image: desktop_logo_img,
    logo_width: desktop_logo_width,
    logo_text: shop.name, 
    is_in_header: true
  -%}

  {% if transparent_visibility != 'none' %}
    {% if transparent_visibility contains 'mobile' %}
      {%- render 'logo',
        class: logo_class_mobile_transparent,
        image: mobile_transparent_logo_img,
        logo_width: mobile_logo_width,
        logo_text: shop.name, 
        is_in_header: true
      -%} 
    {% endif %}
    
    {% if transparent_visibility contains 'desktop' %}
      {%- render 'logo',
        class: logo_class_desktop_transparent,
        image: desktop_transparent_logo_img,
        logo_width: desktop_logo_width,
        logo_text: shop.name, 
        is_in_header: true
      -%} 
    {% endif %}
  {% endif %}
{% endcapture %}

{%- if enable_sticky == 'none' and settings.show_sticky_cart and request.page_type != 'cart' -%}
  <sticky-cart-button>
    {%- if settings.cart_type == 'page_only' or template.name contains 'cart' -%}
      <a
        class='sticky-cart-button unstyled-button section-{{ section.id }}'
        href='{{ routes.cart_url }}'
        aria-label='{{ 'header.screen_reader.aria_label_for_cart_icon' | t }}'
        data-cart-sticky-button
      >
        {{ sticky_cart_icon_variant }}
      </a>
    {%- else -%}
      <sidebar-button>
        <button
          data-sidebar-button
          data-sidebar-id='CartDrawer'
          class='sticky-cart-button unstyled-button'
          aria-label='{{ 'header.screen_reader.aria_label_for_cart_icon' | t }}'
          data-cart-sticky-button
        >
          {{ sticky_cart_icon_variant }}
        </button>
      </sidebar-button>
    {%- endif -%}
  </sticky-cart-button>
{%- endif -%}

{% render 'drawer-menu',
  menu: menu,
  section: section,
  show_account: show_account,
  show_icons_as: show_icons_as,
  icon_style: icon_style,
  icon_stroke_width: icon_stroke_width
%}

{% if request.design_mode %}
  {% style %}
    .mock-drawer-block {
      position: fixed;
      top: 50%;
      bottom: 50%;
      pointer-events: none;
    }
  {% endstyle %}

  <div mock-drawer-blocks>
    {%- for block in section.blocks -%}
      <div class='mock-drawer-block' {{ block.shopify_attributes }} block-id='{{ block.id }}'></div>
    {%- endfor -%}
  </div>
{% endif %}

{% if template.name == 'index' %}
  <h1 class='hidden'>{{ shop.name }}</h1>
{% endif %}

<header-component
  class='header {% if round_level != 'default' %}round-level-{{round_level}}{% endif %} section-{{ section.id }} color-{{ color_scheme }} {% if enable_border == true %}header--bordered{% endif %} {% if enable_shadow %}header--with-shadow{% endif %} container--{{ container_size }} {% if color_container_background %}header__container--with-bg{% endif %}'
  {% if enable_sticky != 'none' %}
    is-sticky='{{enable_sticky}}'
  {% endif %}
  {% if enable_sticky_mobile != 'none' %}
    is-sticky-mobile='{{enable_sticky_mobile}}'
  {% endif %}
  {% if transparent_visibility != 'none' %}
    transparent-visibility='{{transparent_visibility}}'
  {% endif %}
  {% if transparent_visibility != 'none'
    and transparent_opacity == 0.0
    and transparent_blurring == 0.0
    and enable_sticky != 'none'
    or enable_sticky_mobile != 'none'
  %}
    with-sticky-colored
  {% endif %}
  {% if transparent_visibility != 'none' and transparent_background_gradient != blank %}
    data-gradient='{{ transparent_background_gradient }}'
    data-opacity='{{ transparent_opacity }}'
  {% endif %}
  {% if enable_border == false %}
    without-border
  {% endif %}
  {% if menu_trigger == 'click' %}
    with-open-on-click
  {% endif %}
  data-section-id='{{ section.id }}'
  id='header-component'
>
  <div class='header__container'>
    <div class='header__grid {% if menu_type == 'drawer' %}header__grid--drawer header__grid--drawer-{{ grid_class }}{% else %}header__{{ grid_class }}{% endif %} {% if transparent_visibility != "none" %}header__grid--transparent{% endif %}'>
      {% if transparent_visibility contains 'desktop'
        and desktop_transparent_logo_img != blank
        or transparent_visibility contains 'mobile'
        and mobile_transparent_logo_img != blank
      %}
        <div class='header__logos'>
          {{ logos }}
        </div>
      {% else %}
        {{ logos }}
      {% endif %}

      {%- if menu != blank and menu.links != blank and menu_type != 'drawer' -%}
        <div class='header__nav' data-desktop-nav>
          {%- for link in menu.links -%}
            {%- liquid
              assign link_title_downcase = link.title | downcase
              assign has_menu = false
              assign block_index = ''

              for block in section.blocks
                assign mega_nav_item_name_downcase = block.settings.mega_nav_item_name | downcase

                if mega_nav_item_name_downcase == link_title_downcase and block.settings.block_visibility != 'mobile'
                  assign has_menu = true
                  assign block_index = forloop.index0
                  assign block_visibility = block.settings.block_visibility
                endif
              endfor

              assign simple_dropdown = false
              if has_menu == false and menu_type == 'dropdown'
                assign simple_dropdown = true
              endif
              if has_menu == true and block_visibility == 'mobile'
                assign simple_dropdown = true
              endif
            -%}

            {%- if link.links.size > 0 or has_menu == true -%}
              {%- if menu_trigger == 'hover' -%}
                {% if simple_dropdown %}
                  <float-element-btn
                    class='header__dropdown-toggle'
                    data-aria-expanded='false'
                    data-float-element-id='HeaderDropdownMenu-{{ link.handle }}'
                    {% if forloop.index == menu_links_size %}
                      data-is-last-menu-item
                    {% endif %}
                  >
                    <a
                      href='{{ link.url }}'
                      class='underline-text header__item header__item--{{ link.handle }}'
                      data-float-element-trigger='HeaderDropdownMenu-{{ link.handle }}'
                      aria-label='{{ link.title | strip_html | escape }}'
                      menu-title='{{ link.title | strip_html | escape }}'
                    >
                      <span class='underline-text__label'>{{ link.title }}</span>
                    </a>
                  </float-element-btn>
                {% else %}
                  <header-float-element-btn
                    class='header__dropdown-toggle'
                    data-aria-expanded='false'
                    data-float-element-id='HeaderDropdownMenu'
                    {% if has_menu == true or menu_type == 'mega' %}
                      data-header-btn-with-backdrop
                    {% endif %}
                    {% if forloop.index == menu_links_size %}
                      data-is-last-menu-item
                    {% endif %}
                  >
                    <a
                      href='{{ link.url }}'
                      class='underline-text header__item header__item--{{ link.handle }}'
                      data-header-float-element-trigger='HeaderDropdownMenu-{{ link.handle }}'
                      aria-label='{{ link.title | strip_html | escape }}'
                      menu-title='{{ link.title | strip_html | escape }}'
                    >
                      <span class='underline-text__label'>{{ link.title }}</span>
                    </a>
                  </header-float-element-btn>
                {% endif %}

              {%- else -%}
                <dropdown-opener
                  class='header__dropdown-toggle'
                  data-aria-expanded='false'
                  {% if simple_dropdown %}
                    data-float-element-id='HeaderDropdownMenu-{{ link.handle }}'
                  {% else %}
                    data-float-element-id='HeaderDropdownMenu'
                    {% if has_menu == true or menu_type == 'mega' %}
                      data-header-btn-with-backdrop
                    {% endif %}
                  {% endif %}
                  {% if forloop.index == menu_links_size %}
                    data-is-last-menu-item
                  {% endif %}
                >
                  <div
                    class='underline-text header__item header__item--on-click-toggler header__item--{{ link.handle }}'
                    data-float-element-trigger='HeaderDropdownMenu-{{ link.handle }}'
                    menu-title='{{ link.title | strip_html | escape }}'
                  >
                    <span class='underline-text__label'>{{ link.title }}</span>
                    {% render 'icon',
                      icon_name: 'chevron',
                      class: 'header__dropdown-toggle-chevron',
                      stroke_width: 1.5
                    %}
                  </div>
                </dropdown-opener>
              {%- endif -%}

              {% if simple_dropdown %}
                <float-element
                  class='float-element header__dropdown color-{{ color_scheme }}'
                  data-header-dropdown
                  aria-hidden='true'
                  id='HeaderDropdownMenu-{{ link.handle }}'
                  data-section-id='{{ section.id }}'
                  data-with-overlay
                  data-strategy='fixed'
                  data-placement='bottom-start'
                  data-overlay-opacity='10'
                  {% if forloop.index0 == 0 %}
                    data-alignment-axis-offset='{{settings.round_level | minus: 7}}'
                  {% else %}
                    data-alignment-axis-offset='-17'
                  {% endif %}
                  {% if grid == '4' or grid == '5' %}
                    data-main-axis-offset='0'
                  {% endif %}
                  {% if block_index != '' %}
                    block-id='{{ section.blocks[block_index].id }}'
                    {{ section.blocks[block_index].shopify_attributes }}
                  {% endif %}
                >
                  <template data-float-element-template='HeaderDropdownMenu-{{ link.handle }}'>
                    <div class='header__dropdown-menu'>
                      <div class='header__wide-menu'>
                        {%- for childlink in link.links -%}
                          {%- if childlink.links.size > 0 -%}
                            <float-element-btn
                              class='header__menu-item-wrap'
                              data-float-element-id='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                              data-aria-expanded='false'
                              data-childlink
                            >
                              <a
                                href='{{ childlink.url }}'
                                class='header__menu-item header__menu-item--medium {% if childlink.current %} active{% endif %}'
                                data-focus-trap-target
                                data-float-element-trigger='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                                aria-label='{{ childlink.title  | strip_html | escape}}'
                              >
                                <span>
                                  <span class='header__menu-item-label'>
                                    {{ childlink.title }}
                                  </span>
                                </span>
                                {%- render 'icon',
                                  icon_name: 'chevron',
                                  class: 'mega-menu__chevron',
                                  stroke_width: 1.5
                                -%}
                              </a>
                            </float-element-btn>
                            <float-element
                              class='float-element header__menu'
                              data-header-dropdown
                              aria-hidden='true'
                              data-main-axis-offset='{% if settings.round_level != "0" %}{{settings.round_level}}{% else %}32{% endif %}'
                              data-alignment-axis-offset='-24'
                              data-placement='right-start'
                              data-strategy='fixed'
                              id='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                            >
                              <template data-float-element-template='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'>
                                <div>
                                  {%- for grandchildlink in childlink.links -%}
                                    <a
                                      href='{{ grandchildlink.url }}'
                                      class='header__menu-item header__menu-item--small {% if grandchildlink.current %} active{% endif %}'
                                      data-focus-trap-target
                                      aria-label='{{ grandchildlink.title | strip_html | escape }}'
                                    >
                                      <span>
                                        <span class='header__menu-item-label'>
                                          {{ grandchildlink.title }}
                                        </span>
                                      </span>
                                    </a>
                                  {%- endfor -%}
                                </div>
                              </template>
                            </float-element>
                          {%- else -%}
                            <a
                              href='{{ childlink.url }}'
                              class='header__menu-item header__menu-item--medium {% if childlink.current %} active{% endif %}'
                              data-focus-trap-target
                              aria-label='{{ childlink.title | strip_html | escape }}'
                            >
                              <span>
                                <span class='header__menu-item-label'>
                                  {{ childlink.title }}
                                </span>
                              </span>
                            </a>
                          {%- endif -%}
                        {%- endfor -%}
                      </div>
                    </div>
                  </template>
                </float-element>
              {% endif %}

            {%- else -%}
              <a
                href='{{ link.url }}'
                class='underline-text header__item {% if menu_trigger != 'hover' %} header__item--on-click-toggler{% endif %} header__item--{{ link.handle }}'
                aria-label='{{ link.title | strip_html | escape }}'
                menu-title='{{ link.title | strip_html | escape }}'
                {% if forloop.index == menu_links_size %}
                  data-is-last-menu-item
                {% endif %}
              >
                <span class='underline-text__label'>{{ link.title }}</span>
              </a>
            {%- endif -%}
          {%- endfor -%}
        </div>
      {%- endif -%}

      <div class='icon-list header__icons-right'>
        {% if show_language_selector != 'hide' or show_currency_selector != 'hide' %}
          <div class='header__localization-settings'>
            {% if show_language_selector != 'hide' %}
              <div class='localization-dropdown-wrapper {% if show_language_selector != "desktop_and_mobile" %}header-content--{{ show_language_selector }}{% endif %}'>
                <dropdown-opener
                  class='localization-dropdown-toggle header__localization-dropdown-toggle'
                  data-aria-expanded='false'
                  data-trigger='click'
                  data-float-element-id='HeaderDrawerLanguageSelector'
                >
                  {{ localization.language.endonym_name | capitalize }}
                  {% render 'icon',
                    icon_name: 'chevron',
                    class: 'localization-dropdown-toggle__chevron',
                    stroke_width: 1
                  %}
                </dropdown-opener>
                <float-element
                  class='localization-dropdown header__localization-dropdown'
                  aria-hidden='true'
                  id='HeaderDrawerLanguageSelector'
                  data-placement='bottom-end'
                  data-main-axis-offset='25'
                >
                  <template data-float-element-template='HeaderDrawerLanguageSelector'>
                    <div class='localization-dropdown__body'>
                      <localization-selector>
                        {%- form 'localization', id: 'HeaderDrawerLanguageForm' -%}
                          {%- for language in localization.available_languages -%}
                            <button
                              class='localization-dropdown__item link-parent-for-hover {% if localization.language == language %} selected{% endif %}'
                              data-localization-item
                              data-value='{{ language.iso_code }}'
                              data-focus-trap-target
                              aria-label='{{ language.endonym_name | strip_html | escape }}'
                            >
                              <span class='underline-animation'>{{ language.endonym_name | capitalize }}</span>
                            </button>
                          {%- endfor -%}
                          <input
                            type='hidden'
                            name='locale_code'
                            data-localization-input
                            value='{{ localization.language.iso_code }}'
                            id='HeaderDrawerLanguageForm-LocaleCodeInput-{{ form.id }}'
                          >
                        {%- endform -%}
                      </localization-selector>
                    </div>
                  </template>
                </float-element>
              </div>
            {% endif %}

            {% if show_currency_selector != 'hide' %}
              <div class='localization-dropdown-wrapper {% if show_currency_selector != "desktop_and_mobile" %}header-content--{{ show_currency_selector }}{% endif %}'>
                <dropdown-opener
                  class='localization-dropdown-toggle header__localization-dropdown-toggle'
                  data-aria-expanded='false'
                  data-trigger='click'
                  data-float-element-id='HeaderDrawerCurrencySelector'
                >
                  {% if show_currency_flags %}
                    {{
                      localization.country
                      | image_url: width: 20
                      | image_tag: class: 'localization-dropdown-toggle__flag'
                    }}
                  {% endif %}

                  {{ localization.country.currency.iso_code }}
                  {% render 'icon',
                    icon_name: 'chevron',
                    class: 'localization-dropdown-toggle__chevron',
                    stroke_width: 1
                  %}
                </dropdown-opener>
                <float-element
                  class='localization-dropdown header__localization-dropdown'
                  aria-hidden='true'
                  id='HeaderDrawerCurrencySelector'
                  data-placement='bottom-end'
                  data-main-axis-offset='25'
                >
                  <template data-float-element-template='HeaderDrawerCurrencySelector'>
                    <div class='localization-dropdown__body'>
                      <localization-selector>
                        {%- if localization.available_countries.size > 10 %}
                          <div class='localization-dropdown__search-field'>
                            <label class='visually-hidden' for='HeaderDrawerCurrencySelector-Search'>
                              {{- 'general.localization_selector.store_front.search_title' | t -}}
                            </label>

                            <input
                              class='input input--{{ settings.input_style }}'
                              type='text'
                              name='HeaderDrawerCurrencySelector-Search'
                              placeholder='{{- 'general.localization_selector.store_front.search_title' | t -}}'
                              id='HeaderDrawerCurrencySelector-Search'
                              data-localization-search-field
                            >

                            <button
                              type='button'
                              class='unstyled-button localization-dropdown__search-field-reset-btn hidden'
                              aria-label='{{ 'general.search_page.screen_reader.label_for_search_button' | t }}'
                              data-search-field-reset-button
                            >
                              {% render 'icon',
                                icon_name: 'close',
                                class: 'localization-dropdown__search-field-reset-icon',
                                stroke_width: 1.5
                              %}
                            </button>
                          </div>
                        {% endif %}

                        {%- form 'localization', id: 'HeaderDrawerCurrencyForm' -%}
                          {%- for country in localization.available_countries -%}
                            <button
                              data-localization-item
                              class='localization-dropdown__item link-parent-for-hover {% if localization.country == country %} selected{% endif %}'
                              data-value='{{ country.iso_code }}'
                              data-country='{{ country.name | strip_html | escape | downcase }}'
                              data-focus-trap-target
                              aria-label='{{ country.name | strip_html | escape }} {{ country.currency.iso_code | strip_html | escape }}'
                            >
                              {% if show_currency_flags %}
                                {{ country | image_url: width: 22 | image_tag }}
                              {% endif %}

                              <span class='underline-animation'>
                                {{- country.name }}, {{ country.currency.iso_code -}}
                              </span>
                            </button>
                          {%- endfor -%}
                          <input
                            type='hidden'
                            name='country_code'
                            data-localization-input
                            value='{{ localization.country.iso_code }}'
                            id='HeaderDrawerCurrencyForm-CountryCodeInput-{{ form.id }}'
                          >
                        {%- endform -%}
                      </localization-selector>
                    </div>
                  </template>
                </float-element>
              </div>
            {% endif %}
          </div>
        {% endif %}

        {{ search_icon }}

        {{ profile_icon }}

        {%- if settings.cart_type == 'page_only' or template.name contains 'cart' -%}
          <a
            class='unstyled-link icon header__icon'
            href='{{ routes.cart_url }}'
            aria-label='{{ 'header.screen_reader.aria_label_for_cart_icon' | t }}'
          >
            {{ cart_icon_variant }}
          </a>
        {%- else -%}
          <sidebar-button>
            <button
              data-sidebar-button
              data-sidebar-id='CartDrawer'
              class='unstyled-button icon header__icon'
              aria-label='{{ 'header.screen_reader.aria_label_for_cart_icon' | t }}'
            >
              {{ cart_icon_variant }}
            </button>
          </sidebar-button>
        {%- endif -%}
      </div>

      <div class='icon-list header__icons-left'>
        <sidebar-button class='drawer-menu-toggle {% if menu_type == 'drawer' %} drawer-menu-toggle--visible-on-desktop {% endif %}'>
          <button
            class='unstyled-button hamburger header__icon'
            data-sidebar-button
            data-sidebar-id='DrawerMenu'
            id='DrawerMenuToggler'
            aria-label='{{ 'header.screen_reader.aria_label_for_menu_icon' | t }}'
          >
            <svg
              class='hamburger__icon'
              xmlns='http://www.w3.org/2000/svg'
              width='24'
              height='24'
              viewBox='0 0 24 24'
              fill='none'
            >
              <path class='hamburger__line' d="M0 5H20" stroke="currentColor" stroke-width="{{burger_icon_stroke_width}}"/>
              <path class='hamburger__line' d="M0 12H24" stroke="currentColor" stroke-width="{{burger_icon_stroke_width}}"/>
              <path class='hamburger__line' d="M0 19H14" stroke="currentColor" stroke-width="{{burger_icon_stroke_width}}"/>
            </svg>
          </button>
        </sidebar-button>

        {% if account_mobile_position == 'in_header' %}
          {{ profile_icon }}
        {% endif %}
      </div>
    </div>

    <div
      class='header__dropdown-backdrop {% if enable_shadow %}header__dropdown-backdrop--with-shadow{% endif %} {% if color_container_background == false %}header__dropdown-backdrop--full-width{% endif %}'
      data-header-dropdown-backdrop
      style='--gsc-header-backdrop-height: 0px;'
    >
      <header-float-element
        class='float-element header__dropdown color-{{ color_scheme }} header__dropdown--full-width'
        data-header-dropdown
        aria-hidden='true'
        id='HeaderDropdownMenu'
        data-section-id='{{ section.id }}'
        data-with-overlay
        data-strategy='fixed'
        data-placement='bottom-start'
        data-overlay-opacity='10'
        data-floating-element-id='header-component'
      >
        {%- for link in menu.links -%}
          {%- liquid
            assign link_title_downcase = link.title | downcase
            assign has_menu = false
            assign block_index = ''

            for block in section.blocks
              assign mega_nav_item_name_downcase = block.settings.mega_nav_item_name | downcase

              if mega_nav_item_name_downcase == link_title_downcase and block.settings.block_visibility != 'mobile'
                assign has_menu = true
                assign block_index = forloop.index0
                assign block_visibility = block.settings.block_visibility
              endif
            endfor

            assign simple_dropdown = false
            if has_menu == false and menu_type == 'dropdown'
              assign simple_dropdown = true
            endif
            if has_menu == true and block_visibility == 'mobile'
              assign simple_dropdown = true
            endif
          -%}
          {%- if simple_dropdown == false and link.links.size > 0 or has_menu == true -%}
            <template data-header-float-element-template='HeaderDropdownMenu-{{ link.handle }}'>
              <div
                class='header__dropdown-menu {% if has_menu == true or menu_type == 'mega' %} header__dropdown-menu--with-mega-menu{% endif %}'
                {% if block_index != '' %}
                  block-id='{{ section.blocks[block_index].id }}'
                  {{ section.blocks[block_index].shopify_attributes }}
                {% endif %}
              >
                {%- if has_menu == true and menu_type != 'mega' -%}
                  <div class='mega-menu'>
                    <div class='mega-menu__outer-container container container--{{ container_size }}'>
                      <div class='mega-menu__inner-container '>
                        {%- if link != blank and link.links.size > 0 -%}
                          <div class='mega-menu__nav'>
                            <div class='mega-menu__list'>
                              {%- for childlink in link.links -%}
                                {%- if childlink.links.size > 0 -%}
                                  <float-element-btn
                                    class='header__menu-item-wrap'
                                    data-float-element-id='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                                    data-aria-expanded='false'
                                    data-childlink
                                  >
                                    <a
                                      href='{{ childlink.url }}'
                                      class='header__menu-item header__menu-item--medium {% if childlink.current %} active{% endif %}'
                                      data-float-element-trigger='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                                      aria-label='{{ childlink.title | strip_html | escape }}'
                                    >
                                      <span>
                                        <span class='header__menu-item-label'>
                                          {{ childlink.title }}
                                        </span>
                                      </span>
                                      {%- render 'icon',
                                        icon_name: 'chevron',
                                        class: 'mega-menu__chevron',
                                        stroke_width: 1.5
                                      -%}
                                    </a>
                                  </float-element-btn>
                                  <float-element
                                    class='float-element header__menu {% if has_menu == true %} header__menu--inside-full-width-dropdown{% endif %} section-{{ section.id }} color-{{ color_scheme }}'
                                    aria-hidden='true'
                                    data-alignment-axis-offset='-25'
                                    data-main-axis-offset='12'
                                    data-placement='right-start'
                                    data-strategy='fixed'
                                    data-with-portal
                                    id='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                                  >
                                    <template data-float-element-template='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'>
                                      <div>
                                        {%- for grandchildlink in childlink.links -%}
                                          <a
                                            href='{{ grandchildlink.url }}'
                                            class='header__menu-item header__menu-item--small {% if grandchildlink.current %} active{% endif %}'
                                            data-focus-trap-target
                                            aria-label='{{ grandchildlink.title | strip_html | escape }}'
                                          >
                                            <span>
                                              <span class='header__menu-item-label'>
                                                {{ grandchildlink.title }}
                                              </span>
                                            </span>
                                          </a>
                                        {%- endfor -%}
                                      </div>
                                    </template>
                                  </float-element>
                                {%- else -%}
                                  <a
                                    href='{{ childlink.url }}'
                                    class='header__menu-item header__menu-item--medium {% if childlink.current %} active{% endif %}'
                                    data-focus-trap-target
                                    aria-label='{{ childlink.title | strip_html | escape }}'
                                  >
                                    <span>
                                      <span class='header__menu-item-label'>
                                        {{ childlink.title }}
                                      </span>
                                    </span>
                                  </a>
                                {%- endif -%}
                              {%- endfor -%}
                            </div>
                          </div>
                        {%- endif -%}
                        {%- liquid
                          for block in section.blocks
                            assign mega_nav_item_name_downcase = block.settings.mega_nav_item_name | downcase

                            if mega_nav_item_name_downcase == link_title_downcase and block.settings.block_visibility != 'mobile'
                              case block.type
                                when 'menu-1'
                                  render 'header-menu-1', block: block, section_color_scheme: color_scheme
                                  break
                                when 'menu-2'
                                  render 'header-menu-2', block: block
                                  break
                                when 'menu-3'
                                  render 'header-menu-3', block: block, section_color_scheme: color_scheme
                                  break
                              endcase
                            endif
                          endfor
                        -%}
                      </div>
                    </div>
                  </div>
                {%- elsif menu_type == 'mega' -%}
                  <div class='mega-menu mega-dropdown'>
                    <div class='mega-menu__outer-container mega-dropdown__container container container--{{ container_size }}'>
                      <div class='mega-menu__inner-container '>
                        {% if link.links.size > 0 %}
                          <div class='mega-menu__columns {% if grid == '2' or grid == '3' or grid == '5' %}mega-menu__columns--align-left{% endif %}'>
                            {%- for childlink in link.links -%}
                              <div class='mega-dropdown__column'>
                                {%- if childlink.links.size > 0 -%}
                                  <a
                                    href='{{ childlink.url }}'
                                    class='header__menu-item header__menu-item--mega-title'
                                    data-float-element-trigger='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                                    aria-label='{{ childlink.title | strip_html | escape }}'
                                    data-focus-trap-target
                                  >
                                    <span>
                                      <span class='header__menu-item-label'>
                                        {{ childlink.title }}
                                      </span>
                                    </span>
                                  </a>

                                  {%- for grandchildlink in childlink.links -%}
                                    <a
                                      href='{{ grandchildlink.url }}'
                                      class='header__menu-item header__menu-item--mega-subtitle {% if grandchildlink.current %}header__menu-item--mega-subtitle--current{% endif %}'
                                      data-focus-trap-target
                                      aria-label='{{ grandchildlink.title | strip_html | escape }}'
                                    >
                                      <span>
                                        <span class='header__menu-item-label'>
                                          {{ grandchildlink.title }}
                                        </span>
                                      </span>
                                    </a>
                                  {%- endfor -%}
                                {%- else -%}
                                  <a
                                    href='{{ childlink.url }}'
                                    class='header__menu-item header__menu-item--mega-title'
                                    data-focus-trap-target
                                    aria-label='{{ childlink.title | strip_html | escape }}'
                                  >
                                    <span>
                                      <span class='header__menu-item-label'>
                                        {{ childlink.title }}
                                      </span>
                                    </span>
                                  </a>
                                {%- endif -%}
                              </div>
                            {%- endfor -%}
                          </div>
                        {% endif %}

                        {%- liquid
                          for block in section.blocks
                            assign mega_nav_item_name_downcase = block.settings.mega_nav_item_name | downcase

                            if mega_nav_item_name_downcase == link_title_downcase and block.settings.block_visibility != 'mobile'
                              case block.type
                                when 'menu-1'
                                  render 'header-menu-1', block: block, section_color_scheme: color_scheme
                                  break
                                when 'menu-2'
                                  render 'header-menu-2', block: block
                                  break
                                when 'menu-3'
                                  render 'header-menu-3', block: block, section_color_scheme: color_scheme
                                  break
                              endcase
                            endif
                          endfor
                        -%}
                      </div>
                    </div>
                  </div>
                {%- elsif menu_type != 'mega' and link.levels == 2 -%}
                  <div class='header__wide-menu'>
                    {%- for childlink in link.links -%}
                      {%- if childlink.links.size > 0 -%}
                        <float-element-btn
                          class='header__menu-item-wrap'
                          data-float-element-id='MegaMenuNav-{{ link.handle }}-{{ childlink.handle }}'
                          data-aria-expanded='false'
                          data-childlink
                        >
