{%- liquid
  assign options = section.settings

  assign heading_size = options.heading_size
  assign body_size = options.body_size

  assign title = options.title
  assign caption = options.caption
  assign text = options.text

  assign btn_text = options.btn_text
  assign btn_url = options.btn_url
  assign btn_style = options.btn_style

  assign extra_btn_text = options.extra_btn_text
  assign extra_btn_url = options.extra_btn_url
  assign extra_btn_style = options.extra_btn_style

  assign round_level = options.round_level
  assign mobile_ratio = options.mobile_ratio
  assign desktop_ratio = options.desktop_ratio
  assign add_desktop_side_paddings = options.add_desktop_side_paddings
  assign add_mobile_side_paddings = options.add_mobile_side_paddings
  assign section_spacing_top = options.section_spacing_top
  assign section_spacing_bottom = options.section_spacing_bottom
  assign container_size = options.container_size

  assign mobile_content_position = options.mobile_content_position
  assign desktop_content_position = options.desktop_content_position

  assign desktop_content_align = options.desktop_content_align
  assign mobile_content_align = options.mobile_content_align

  assign content_max_width = options.content_max_width | divided_by: 10.0
  assign content_paddings_size = options.content_paddings_size

  assign desktop_media_type = options.desktop_media_type
  assign desktop_image = options.desktop_image
  assign desktop_video = options.desktop_video

  assign mobile_media_type = options.mobile_media_type
  assign mobile_image = options.mobile_image
  assign mobile_video = options.mobile_video

  assign mobile_content_background = options.mobile_content_background
  assign mobile_outline_border_thickness = options.mobile_outline_border_thickness
  assign mobile_background_opacity = options.mobile_background_opacity
  assign mobile_background_blur = options.mobile_background_blur
  assign mobile_shadow_background_color = options.mobile_shadow_background_color

  assign desktop_content_background = options.desktop_content_background
  assign desktop_outline_border_thickness = options.desktop_outline_border_thickness
  assign desktop_background_opacity = options.desktop_background_opacity
  assign desktop_background_blur = options.desktop_background_blur
  assign desktop_shadow_background_color = options.desktop_shadow_background_color

  assign disable_lazy_loading = options.disable_lazy_loading
-%}

{%- liquid
  assign color_scheme = settings.default_color_scheme
  if options.color_scheme_variant == 'own'
    assign color_scheme = options.color_scheme
  endif

  assign background_color = color_scheme.settings.background
  assign foreground_color = color_scheme.settings.foreground
  assign text_color = color_scheme.settings.text

  if options.color_scheme_variant == 'custom'
    assign background_color = section.settings.custom_background_color
    assign foreground_color = section.settings.custom_foreground_color
    assign text_color = section.settings.custom_text_color

    assign btn_style = 'primary'
    if options.btn_style == 'link'
      assign btn_style = 'link'
    endif

    assign extra_btn_style = 'secondary'
    if options.extra_btn_style == 'link'
      assign extra_btn_style = 'link'
    endif
  endif

  assign desktop_enable_overlay = options.desktop_enable_overlay
  assign desktop_overlay_opacity = options.desktop_overlay_opacity | divided_by: 100.00
  assign mobile_enable_overlay = options.mobile_enable_overlay
  assign mobile_overlay_opacity = options.mobile_overlay_opacity | divided_by: 100.00

  assign with_overlay = true
  if desktop_enable_overlay == false and mobile_enable_overlay == false
    assign with_overlay = false
  endif
-%}

{%- liquid
  assign show_background = section.settings.show_background
  assign color_container_background = false

  if show_background == false and settings.default_color_scheme.settings.background != color_scheme.settings.background
    assign color_container_background = true
  endif

  if show_background == false and options.color_scheme_variant == 'custom' and settings.default_color_scheme.settings.background != section.settings.custom_background_color
    assign color_container_background = true
  endif
-%}

{% capture css %}
  {% style %}
    .section-{{section.id}}{
      --gsc-section-ratio: {{ mobile_ratio | times: 100 }}%;
      --gsc-content-width: {{ content_max_width }}rem;
      --gsc-content-align: {{ mobile_content_align }};
      --gsc-section-spacing-top: var(--gsc-padding-block-{{ section_spacing_top }});
      --gsc-section-spacing-bottom: var(--gsc-padding-block-{{ section_spacing_bottom }});
      --gsc-content-paddings: var(--gsc-content-padding-{{ content_paddings_size }});
      
      {% if options.color_scheme_variant == 'custom' %}
        --gsc-primary-button-text-color: {{ section.settings.custom_first_button_text_color }};
        --gsc-primary-button-bg-color: {{ section.settings.custom_first_button_bg_color }};
        --gsc-primary-button-bg-color-rgb: {{ section.settings.custom_first_button_bg_color.red }}, {{ section.settings.custom_first_button_bg_color.green }}, {{ section.settings.custom_first_button_bg_color.blue }};
        --gsc-primary-button-bg-hover: {% if section.settings.custom_first_button_bg_color.lightness < 50 %}{{ section.settings.custom_first_button_bg_color | color_lighten: 8 }}{% else %}{{ section.settings.custom_first_button_bg_color | color_darken: 8 }}{% endif %};
        --gsc-primary-button-border-color: {% if section.settings.custom_first_button_border_color.alpha > 0 %}{{ section.settings.custom_first_button_border_color }}{% else %}transparent{% endif %};

        --gsc-secondary-button-text-color: {{ section.settings.custom_second_button_text_color }};
        --gsc-secondary-button-bg-color: {{ section.settings.custom_second_button_bg_color }};
        --gsc-secondary-button-bg-color-rgb: {{ section.settings.custom_second_button_bg_color.red }}, {{ section.settings.custom_second_button_bg_color.green }}, {{ section.settings.custom_second_button_bg_color.blue }};
        --gsc-secondary-button-bg-hover: {% if section.settings.custom_second_button_bg_color.lightness < 50 %}{{ section.settings.custom_second_button_bg_color | color_lighten: 8 }}{% else %}{{ section.settings.custom_second_button_bg_color | color_darken: 8 }}{% endif %};
        --gsc-secondary-button-border-color: {% if section.settings.custom_second_button_border_color.alpha > 0 %}{{ section.settings.custom_second_button_border_color }}{% else %}transparent{% endif %};

        --gsc-link-button-text-color: {{ section.settings.custom_link_color }};
        
        --gsc-background-color-100: {{ background_color }};
        --gsc-content-solid-background-rgb: {{ foreground_color.rgb | replace: ' ', ',' }};
        --gsc-text-color-100: {{ text_color }};
        --gsc-text-color: {{ text_color.rgb | replace: ' ', ',' }};
      {% endif %}

      {% if mobile_enable_overlay %}
        --gsc-overlay-color-rgb: {{ background_color.rgb | replace: ' ', ',' }};
        --gsc-overlay-opacity: {{mobile_overlay_opacity}};
      {% else %}
        --gsc-overlay-color-rgb: 0,0,0;
        --gsc-overlay-opacity: 0;
      {% endif %} 

      {% if mobile_content_background == 'solid' %}
        --gsc-content-solid-opacity: {{ mobile_background_opacity }}%;
        --gsc-content-solid-blur: {{ mobile_background_blur }}px;
      {% endif %}

      {% if mobile_content_background == 'outline' %}
        --gsc-content-border-width: {{ mobile_outline_border_thickness }}px;
      {% endif %}

      {% if mobile_content_background == 'shadow' %}
        --gsc-content-shadow-color: {{ mobile_shadow_background_color }};
      {% endif %}
    }

    .section-{{ section.id }} .image-banner__content-wrap {
      {{ mobile_content_position }}
    }

    @media {% render 'media-queries', screen: 'md' %} {
        .section-{{ section.id }} .image-banner__content-wrap {
            {{ desktop_content_position }}
        }

        .section-{{ section.id }}{
            --gsc-section-ratio: {{ desktop_ratio | times: 100 }}%;
            --gsc-content-align: {{ desktop_content_align }};

            {% if desktop_enable_overlay %}
              --gsc-overlay-color-rgb: {{ background_color.rgb | replace: ' ', ',' }};
              --gsc-overlay-opacity: {{desktop_overlay_opacity}};
            {% else %}
              --gsc-overlay-color-rgb: 0,0,0;
              --gsc-overlay-opacity: 0;
            {% endif %}

            {% if desktop_content_background == 'solid' %}
              --gsc-content-solid-opacity: {{ desktop_background_opacity }}%;
              --gsc-content-solid-blur: {{ desktop_background_blur }}px;
            {% endif %}

            {% if desktop_content_background == 'outline' %}
              --gsc-content-border-width: {{ desktop_outline_border_thickness }}px;
            {% endif %}

            {% if desktop_content_background == 'shadow' %}
              --gsc-content-shadow-color: {{ desktop_shadow_background_color }};
            {% endif %}
        }
    }
  {% endstyle %}
{% endcapture %}

{% render 'inline-css-minifier', css: css %}

<div class='section-{{ section.id }} {% if round_level != 'default' %}round-level-{{round_level}}{% endif %} section color-{{ color_scheme }} spacing-block {% if add_mobile_side_paddings %}spacing-inline-mobile{% endif %} {% if add_desktop_side_paddings %}spacing-inline-desktop{% endif %} {% if color_container_background == false %}section-background{% endif %}'>
  <div class='container container--{{ container_size }} image-banner__outer-container {% if color_container_background %}container-background--no-paddings{% endif %}'>
    <div class='image-banner'>
      <div class='image-banner__container {% if mobile_content_background != 'none' %} image-banner__container--mobile-content-with-background {% endif %}'>
        {%- if btn_text == blank and extra_btn_text == blank -%}
          {%- if btn_url.url != blank or extra_btn_url.url != blank -%}
            <a
              class='image-banner__link'
              href='{% if btn_url.url != blank %}{{ btn_url.url }}{% else %}{{ extra_btn_url.url }}{% endif %}'
              aria-label='{% if btn_text != blank %}{{ btn_text | strip_html | escape }}{% elsif extra_btn_text != blank %}{{ extra_btn_text | strip_html | escape }}{% elsif btn_url.title != blank %}{{ btn_url.title | strip_html | escape }}{% elsif extra_btn_url.title != blank %}{{ extra_btn_url.title | strip_html | escape }}{% else %}#{% endif %}'
            ></a>
          {%- endif -%}
        {%- endif -%}

        {%- render 'image-banner-content',
          heading_size: heading_size,
          body_size: body_size,
          title: title,
          caption: caption,
          text: text,
          btn_text: btn_text,
          btn_url: btn_url,
          extra_btn_text: extra_btn_text,
          extra_btn_url: extra_btn_url,
          btn_style: btn_style,
          extra_btn_style: extra_btn_style,
          mobile_content_background: mobile_content_background,
          desktop_content_background: desktop_content_background
        -%}

        {%- render 'media-with-art-direction',
          with_overlay: with_overlay,
          with_placeholder: true,
          class: 'image-banner__media',
          desktop_media_type: desktop_media_type,
          mobile_media_type: mobile_media_type,
          desktop_img: desktop_image,
          mobile_img: mobile_image,
          desktop_video: desktop_video,
          mobile_video: mobile_video,
          is_eager_loading: disable_lazy_loading
        -%}
      </div>
    </div>
  </div>
</div>

{% render 'save-section-data-in-global-scope', section: section %}

