{% liquid
  assign color_schemes = settings.color_schemes
  assign default_color_scheme = settings.default_color_scheme
%}

{%- capture css -%}
  {% style %}
    :root {
      /* product card */

      --gsc-product-card-title-font-family: {{ settings.product_card_font_family.family }}, {{ settings.product_card_font_family.fallback_families }};
      --gsc-product-card-title-font-style: {{ settings.product_card_font_family.style }};
      --gsc-product-card-title-font-weight: {{ settings.product_card_font_family.weight }};
      --gsc-product-card-title-transform: {{ settings.product_card_font_uppercase }};
      --gsc-product-card-title-letter-spacing: {{ settings.product_card_font_letter_spacing }}px;
      --gsc-product-card-title-font-size: calc(1.4rem * {{ settings.product_card_font_scale | times: 0.01 }});
      --gsc-product-card-title-line-height: calc(22 / 14);

      --gsc-product-card-price-font-family: {{ settings.product_card_price_font_family.family }}, {{ settings.product_card_price_font_family.fallback_families }};
      --gsc-product-card-price-font-size: var(--gsc-product-card-price-font-size-{{- settings.product_card_price_size -}});
      --gsc-product-card-price-line-height: var(--gsc-product-card-price-line-height-{{- settings.product_card_price_size -}});
      --gsc-product-card-price-font-weight: {{ settings.product_card_price_font_family.weight }};      

      {% if settings.enable_product_darken_background == true %}
        --gsc-product-card-media-bg-color: {{ settings.darken_background_color }};
        --gsc-product-card-media-bg-mix-mode: {{ settings.darken_background_mix_mode }};
      {% endif %}

      {% if settings.product_sale_badge_bg_gradient != blank %}
        --gsc-product-sale-badge-background: {{ settings.product_sale_badge_bg_gradient }};
      {% else %}
        --gsc-product-sale-badge-background: rgba({{ settings.product_sale_badge_bg.red }}, {{ settings.product_sale_badge_bg.green }}, {{ settings.product_sale_badge_bg.blue }}, {{ settings.product_sale_badge_bg.alpha }});         
      {% endif %}
      --gsc-product-sale-badge-color: rgba({{ settings.product_sale_badge_color.red }}, {{ settings.product_sale_badge_color.green }}, {{ settings.product_sale_badge_color.blue }}, {{ settings.product_sale_badge_color.alpha }});

      {% if settings.product_card_sold_out_badge_bg_gradient != blank %}
        --gsc-product-sold-out-badge-background: {{ settings.product_card_sold_out_badge_bg_gradient }};
      {% else %}
        --gsc-product-sold-out-badge-background: rgb({{ settings.product_card_sold_out_badge_bg.red }}, {{ settings.product_card_sold_out_badge_bg.green }}, {{ settings.product_card_sold_out_badge_bg.blue }});
      {% endif %}
      --gsc-product-sold-out-badge-color: {{ settings.product_card_sold_out_badge_color.red }}, {{ settings.product_card_sold_out_badge_color.green }}, {{ settings.product_card_sold_out_badge_color.blue }};

      {% if settings.product_card_custom_badge_1_bg_gradient != blank %}
        --gsc-product-custom-badge-1-background: {{ settings.product_card_custom_badge_1_bg_gradient }};
      {% else %}
        --gsc-product-custom-badge-1-background:  rgba({{ settings.product_card_custom_badge_1_bg.red }}, {{ settings.product_card_custom_badge_1_bg.green }}, {{ settings.product_card_custom_badge_1_bg.blue }}, {{ settings.product_card_custom_badge_1_bg.alpha }});
      {% endif %}
      --gsc-product-custom-badge-1-color:  rgba({{ settings.product_card_custom_badge_1_color.red }}, {{ settings.product_card_custom_badge_1_color.green }}, {{ settings.product_card_custom_badge_1_color.blue }}, {{ settings.product_card_custom_badge_1_color.alpha }});
      
      {% if settings.product_card_custom_badge_2_bg_gradient != blank %}
        --gsc-product-custom-badge-2-background: {{ settings.product_card_custom_badge_2_bg_gradient }};
      {% else %}
        --gsc-product-custom-badge-2-background:  rgba({{ settings.product_card_custom_badge_2_bg.red }}, {{ settings.product_card_custom_badge_2_bg.green }}, {{ settings.product_card_custom_badge_2_bg.blue }}, {{ settings.product_card_custom_badge_2_bg.alpha }});
      {% endif %}
      --gsc-product-custom-badge-2-color:  rgba({{ settings.product_card_custom_badge_2_color.red }}, {{ settings.product_card_custom_badge_2_color.green }}, {{ settings.product_card_custom_badge_2_color.blue }}, {{ settings.product_card_custom_badge_2_color.alpha }});
      
      {% if settings.product_card_custom_badge_3_bg_gradient != blank %}
        --gsc-product-custom-badge-3-background: {{ settings.product_card_custom_badge_3_bg_gradient }};
      {% else %}
        --gsc-product-custom-badge-3-background:  rgba({{ settings.product_card_custom_badge_3_bg.red }}, {{ settings.product_card_custom_badge_3_bg.green }}, {{ settings.product_card_custom_badge_3_bg.blue }}, {{ settings.product_card_custom_badge_3_bg.alpha }});
      {% endif %}
      --gsc-product-custom-badge-3-color:  rgba({{ settings.product_card_custom_badge_3_color.red }}, {{ settings.product_card_custom_badge_3_color.green }}, {{ settings.product_card_custom_badge_3_color.blue }}, {{ settings.product_card_custom_badge_3_color.alpha }});
      
      --gsc-rating-stars-color: {{settings.product_card_rating_color}};
      
      /* product and product card badges */
      
      --gsc-product-highlight-price-color:   rgba({{ settings.product_highlight_sale_price_color.red }}, {{ settings.product_highlight_sale_price_color.green }}, {{ settings.product_highlight_sale_price_color.blue }}, {{ settings.product_highlight_sale_price_color.alpha }});
      
      {% if settings.product_sale_amount_badge_bg_gradient != blank %}
        --gsc-product-sale-amount-badge-background: {{ settings.product_sale_amount_badge_bg_gradient }};
      {% else %}
        --gsc-product-sale-amount-badge-background:  rgba({{ settings.product_sale_amount_badge_bg.red }}, {{ settings.product_sale_amount_badge_bg.green }}, {{ settings.product_sale_amount_badge_bg.blue }}, {{ settings.product_sale_amount_badge_bg.alpha }});
      {% endif %}
      --gsc-product-sale-amount-badge-color:   rgba({{ settings.product_sale_badge_amount_color.red }}, {{ settings.product_sale_badge_amount_color.green }}, {{ settings.product_sale_badge_amount_color.blue }}, {{ settings.product_sale_badge_amount_color.alpha }});

      /* cart icon in header */

      {% if settings.cart_status_background_gradient != blank %}
        --gsc-header-cart-icon-background-color: {{ settings.cart_status_background_gradient }};
      {% else %}
        --gsc-header-cart-icon-background-color: {{ settings.cart_status_background_color }};
      {% endif %}
      --gsc-header-cart-icon-text-color: {{ settings.cart_status_text_color }};

      /* shipping bar */

      --gsc-free-shipping-bar-message-color: {{ settings.cart_free_shipping_message_color }};
      --gsc-free-shipping-bar-size: {{ settings.cart_free_shipping_bar_size }}px;
      --gsc-free-shipping-bar-bg-high: {{ settings.cart_free_shipping_bar_bg }};      
      {% if settings.cart_free_shipping_bar_gradient != blank %}
        --gsc-free-shipping-bar-color: {{ settings.cart_free_shipping_bar_gradient }};
      {% else %}
        --gsc-free-shipping-bar-color: {{ settings.cart_free_shipping_bar_color }};
      {% endif %}

      /* headings */

      --gsc-headings-font-family: {{ settings.heading_font_family.family }}, {{ settings.heading_font_family.fallback_families }};
      --gsc-headings-font-style: {{ settings.heading_font_family.style }};
      --gsc-headings-font-weight: {{ settings.heading_font_family.weight }};
      --gsc-headings-text-transform: {{ settings.heading_font_uppercase }};
      --gsc-headings-letter-spacing: {{ settings.heading_font_letter_spacing }}px;
      --gsc-headings-word-spacing: 0rem;
      --gsc-headings-font-scale: {{ settings.heading_font_scale | times: 0.01 }};

      /* body text */

      --gsc-body-font-family: {{ settings.body_font_family.family }}, {{ settings.body_font_family.fallback_families }};
      --gsc-body-font-style: {{ settings.body_font_family.style }};
      --gsc-body-font-weight: {{ settings.body_font_family.weight }};
      --gsc-body-font-scale: {{ settings.body_font_scale | times: 0.01 }};
      --gsc-body-font-opacity: {{ settings.body_font_opacity }}%;
      --gsc-body-font-letter-spacing: {{ settings.body_font_letter_spacing }}px;

      /* button */

      --gsc-button-font-family: {{ settings.button_font_family.family }}, {{ settings.button_font_family.fallback_families }};
      --gsc-button-font-style: {{ settings.button_font_family.style }};
      --gsc-button-font-weight: {{ settings.button_font_family.weight }};
      --gsc-button-letter-spacing: {{ settings.button_font_letter_spacing }}px;
      --gsc-button-font-size: calc(1.4rem * {{ settings.button_font_scale | times: 0.01 }});
      --gsc-button-line-height: calc(22 / 14);
      --gsc-button-text-transform: {{ settings.button_font_uppercase }};

      /* menu */

      --gsc-header-font-family: {{ settings.header_font_family.family }}, {{ settings.header_font_family.fallback_families }};
      --gsc-header-font-style: {{ settings.header_font_family.style }};
      --gsc-header-font-weight: {{ settings.header_font_family.weight }};
      --gsc-header-font-case: {{ settings.header_font_case }};
      --gsc-header-first-level-font-size: calc(var(--gsc-fz-11) * {{ settings.first_level_header_font_scale | times: 0.01 }});
      --gsc-header-first-level-letter-spacing: {{ settings.first_level_header_font_letter_spacing }}px;
      --gsc-header-second-level-font-size: calc(var(--gsc-fz-11) * {{ settings.second_level_header_font_scale | times: 0.01 }});
      --gsc-header-third-level-font-size: calc(var(--gsc-fz-11) * {{ settings.third_level_header_font_scale | times: 0.01 }});

      --gsc-drawer-font-family: {{ settings.drawer_font_family.family }}, {{ settings.drawer_font_family.fallback_families }};
      --gsc-drawer-font-style: {{ settings.drawer_font_family.style }};
      --gsc-drawer-font-weight: {{ settings.drawer_font_family.weight }};
      --gsc-drawer-font-case: {{ settings.drawer_font_case }};
      --gsc-drawer-first-level-font-size: calc(var(--gsc-fz-11) * {{ settings.first_level_drawer_font_scale | times: 0.01 }});
      --gsc-drawer-first-level-letter-spacing: {{ settings.first_level_drawer_font_letter_spacing }}px;
      --gsc-drawer-second-level-font-size: calc(var(--gsc-fz-11) * {{ settings.second_level_drawer_font_scale | times: 0.01 }});
      --gsc-drawer-third-level-font-size: calc(var(--gsc-fz-11) * {{ settings.third_level_drawer_font_scale | times: 0.01 }});
      
      --gsc-menu-line-height: calc(24 / 16);
      
      /* header icons */

      --gsc-header-icons-font-family: {{ settings.header_icons_font_family.family }}, {{ settings.header_icons_font_family.fallback_families }};
      --gsc-header-icons-font-style: {{ settings.header_icons_font_family.style }};
      --gsc-header-icons-font-weight: {{ settings.header_icons_font_family.weight }};
      --gsc-header-icons-font-letter-spacing: {{ settings.header_icons_font_letter_spacing }}px;
      --gsc-header-icons-font-size: calc(var(--gsc-fz-11) * {{ settings.header_icons_font_scale | times: 0.01 }});
      --gsc-header-icons-line-height: calc(var(--gsc-header-icons-font-size) * 1.5);
      --gsc-header-icons-gap: calc(10px * {{ settings.header_icons_font_scale | times: 0.01 }});
      --gsc-header-icons-text-transform: {{ settings.header_icons_font_uppercase }};

      /* icons */

      --gsc-blockquote-icon: url({{ 'blockquote-icon.png' | asset_img_url: width: 64, height: 64 }});

      /* container */
      
      --gsc-page-width: {{ settings.page_width }}px;
      --gsc-aside-padding-size: var(--gsc-padding-inline-{{ settings.aside_padding_size }});
      --gsc-modals-and-header-paddings: min(20px, var(--gsc-padding-inline-{{ settings.aside_padding_size }}));

      /* drawers and modals */
      
      --gsc-drawer-transition-duration: 0.5s;
      --gsc-drawer-transition-timing: cubic-bezier(0.24, 0.25, 0, 1);

      {% if settings.overlay_opacity == 0 and settings.overlay_blur == 0 %}
        --gsc-drawer-modal-shadow: drop-shadow(0 1.2rem 4.8rem rgb(0, 0, 0, 12%));
      {% else %}
        --gsc-drawer-modal-shadow: none;
      {% endif %}

      {% case settings.default_img_ratio %}
        {% when 'square' %}  
          --gsc-default-card-image-ratio: var(--gsc-square-image-ratio);
          --gsc-default-card-image-ratio-numeric: var(--gsc-square-image-ratio-numeric);
        {% when 'portrait' %}
          --gsc-default-card-image-ratio: var(--gsc-portrait-image-ratio);
          --gsc-default-card-image-ratio-numeric: var(--gsc-portrait-image-ratio-numeric);
        {% when 'portrait-xl' %}
          --gsc-default-card-image-ratio: var(--gsc-large-portrait-image-ratio);
          --gsc-default-card-image-ratio-numeric: var(--gsc-large-portrait-image-ratio-numeric);
        {% when 'landscape' %}
          --gsc-default-card-image-ratio: var(--gsc-landscape-image-ratio);
          --gsc-default-card-image-ratio-numeric: var(--gsc-landscape-image-ratio-numeric);
      {% endcase %}
          
      --gsc-default-shape-fill-mode: {{ settings.card_img_resize  }};
    }

    {% if settings.body_font_family.weight != '400' %}
      .body-font-weight-from-global-settings {
        font-weight: var(--gsc-body-font-weight);

        --gsc-font-weight-bold: var(--gsc-body-font-weight);
        --gsc-font-weight-semibold: var(--gsc-body-font-weight);
        --gsc-font-weight-medium: var(--gsc-body-font-weight);
      }
    {% endif %}

    body {
      --gsc-body-overlay-color: {{ settings.overlay_color.red }} {{ settings.overlay_color.green }} {{ settings.overlay_color.blue }} / {{ settings.overlay_color.alpha }};
      --gsc-body-overlay-opacity: {{ settings.overlay_color.alpha | times: 100 | minus: 100 | abs }}%;
      --gsc-body-overlay-blur: {{ settings.overlay_blur | divided_by: 2.0 }}px;

      --gsc-primary-button-border-width: {{ settings.button_primary_border_width }}px;
      --gsc-secondary-button-border-width: {{ settings.button_secondary_border_width }}px;
      --gsc-tertiary-button-border-width: {{ settings.button_tertiary_border_width }}px;
    }
    
    {% for color_scheme in color_schemes %}
      {% liquid
        assign scheme_settings = color_scheme.settings

        assign button_darken = 8
        assign button_lighten = 8

        assign input_darken = 3
        assign input_lighten = 3

        assign input_controls_darken = 5
        assign input_controls_lighten = 5

        assign bg_darken = 5
        assign bg_lighten = 5
      %}

      {% if default_color_scheme == color_scheme %}body,{% endif %}
      .color-{{color_scheme}} {
        --gsc-text-color: {{ scheme_settings.text.red }}, {{ scheme_settings.text.green }}, {{ scheme_settings.text.blue }};
        --gsc-background-color: {{ scheme_settings.background.red }}, {{ scheme_settings.background.green }}, {{ scheme_settings.background.blue }};
        --gsc-background-color-hover: {% if scheme_settings.background.lightness < 50 %}{{ scheme_settings.background | color_lighten: bg_lighten }}{% else %}{{ scheme_settings.background | color_darken: bg_darken }}{% endif %};
        --gsc-foreground-color: {{ scheme_settings.foreground.red }}, {{ scheme_settings.foreground.green }}, {{ scheme_settings.foreground.blue }};
        --gsc-border-color: {{ scheme_settings.border.red }}, {{ scheme_settings.border.green }}, {{ scheme_settings.border.blue }};

        --gsc-primary-button-text-color: {{ scheme_settings.button_primary_text }};
        --gsc-primary-button-bg-color: {{ scheme_settings.button_primary_background }};
        --gsc-primary-button-bg-color-rgb: {{ scheme_settings.button_primary_background.red }}, {{ scheme_settings.button_primary_background.green }}, {{ scheme_settings.button_primary_background.blue }};
        --gsc-primary-button-bg-hover: {% if scheme_settings.button_primary_background.lightness < 50 %}{{ scheme_settings.button_primary_background | color_lighten: button_lighten }}{% else %}{{ scheme_settings.button_primary_background | color_darken: button_darken }}{% endif %};
        --gsc-primary-button-border-color: {% if scheme_settings.button_primary_border.alpha > 0 %}{{ scheme_settings.button_primary_border }}{% else %}transparent{% endif %};
        --gsc-primary-button-blur: {% if scheme_settings.button_primary_background.alpha == 0 %}unset{% else %}{{ settings.button_primary_blur }}px{% endif %};

        --gsc-secondary-button-text-color: {{ scheme_settings.button_secondary_text }};
        --gsc-secondary-button-bg-color: {{ scheme_settings.button_secondary_background }};
        --gsc-secondary-button-bg-color-rgb: {{ scheme_settings.button_secondary_background.red }}, {{ scheme_settings.button_secondary_background.green }}, {{ scheme_settings.button_secondary_background.blue }};
        --gsc-secondary-button-bg-hover: {% if scheme_settings.button_secondary_background.lightness < 50 %}{{ scheme_settings.button_secondary_background | color_lighten: button_lighten }}{% else %}{{ scheme_settings.button_secondary_background | color_darken: button_darken }}{% endif %};
        --gsc-secondary-button-border-color: {% if scheme_settings.button_secondary_border.alpha > 0 %}{{ scheme_settings.button_secondary_border }}{% else %}transparent{% endif %};
        --gsc-secondary-button-blur: {% if scheme_settings.button_secondary_background.alpha == 0 %}unset{% else %}{{ settings.button_secondary_blur }}px{% endif %};

        --gsc-tertiary-button-text-color: {{ scheme_settings.button_tertiary_text }};
        --gsc-tertiary-button-bg-color: {{ scheme_settings.button_tertiary_background }};
        --gsc-tertiary-button-bg-color-rgb: {{ scheme_settings.button_tertiary_background.red }}, {{ scheme_settings.button_tertiary_background.green }}, {{ scheme_settings.button_tertiary_background.blue }};
        --gsc-tertiary-button-bg-hover: {% if scheme_settings.button_tertiary_background.lightness < 50 %}{{ scheme_settings.button_tertiary_background | color_lighten: button_lighten }}{% else %}{{ scheme_settings.button_tertiary_background | color_darken: button_darken }}{% endif %};
        --gsc-tertiary-button-border-color: {% if scheme_settings.button_tertiary_border.alpha > 0 %}{{ scheme_settings.button_tertiary_border }}{% else %}transparent{% endif %};
        --gsc-tertiary-button-blur: {% if scheme_settings.button_tertiary_background.alpha == 0 %}unset{% else %}{{ settings.button_tertiary_blur }}px{% endif %};

        --gsc-link-button-text-color: {{ scheme_settings.button_link_text }};

        --gsc-inputs-text-color: {{ scheme_settings.inputs_text }};
        --gsc-inputs-text-color-rgb: {{ scheme_settings.inputs_text.red }}, {{ scheme_settings.inputs_text.green }}, {{ scheme_settings.inputs_text.blue }};
        --gsc-inputs-bg-color: {{ scheme_settings.inputs_background }};
        --gsc-inputs-bg-color-rgb: {{ scheme_settings.inputs_background.red }}, {{ scheme_settings.inputs_background.green }}, {{ scheme_settings.inputs_background.blue }};
        --gsc-inputs-bg-color-hover: {% if scheme_settings.inputs_background.lightness < 50 %}{{ scheme_settings.inputs_background | color_lighten: input_lighten }}{% else %}{{ scheme_settings.inputs_background | color_darken: input_darken }}{% endif %};
        --gsc-inputs-controls-bg-color-hover: {% if scheme_settings.inputs_background.lightness < 50 %}{{ scheme_settings.inputs_background | color_lighten: input_controls_lighten }}{% else %}{{ scheme_settings.inputs_background | color_darken: input_controls_darken }}{% endif %};
        --gsc-inputs-border-color: {{ scheme_settings.inputs_border }};
        --gsc-inputs-border-color-rgb: {{ scheme_settings.inputs_border.red }}, {{ scheme_settings.inputs_border.green }}, {{ scheme_settings.inputs_border.blue }};
      }
    {% endfor %}

      body,
    {% for color_scheme in color_schemes %}
      .color-{{color_scheme}}{% if forloop.last == false %},{% endif %}
    {% endfor %} {
        --gsc-white-color: 255, 255, 255;
        --gsc-black-color: 0, 0, 0;
        --gsc-green-color: 83, 158, 58;
        --gsc-yellow-color: 255, 165, 29;
        --gsc-red-color: 195, 59, 59;
        --gsc-blue-color: 62, 46, 255;

        --gsc-text-color-0: rgba(var(--gsc-text-color), 0);
        --gsc-text-color-5: rgba(var(--gsc-text-color), 0.05);
        --gsc-text-color-10: rgba(var(--gsc-text-color), 0.1);
        --gsc-text-color-15: rgba(var(--gsc-text-color), 0.15);
        --gsc-text-color-20: rgba(var(--gsc-text-color), 0.2);
        --gsc-text-color-25: rgba(var(--gsc-text-color), 0.25);
        --gsc-text-color-30: rgba(var(--gsc-text-color), 0.3);
        --gsc-text-color-35: rgba(var(--gsc-text-color), 0.35);
        --gsc-text-color-40: rgba(var(--gsc-text-color), 0.4);
        --gsc-text-color-45: rgba(var(--gsc-text-color), 0.45);
        --gsc-text-color-50: rgba(var(--gsc-text-color), 0.5);
        --gsc-text-color-55: rgba(var(--gsc-text-color), 0.55);
        --gsc-text-color-60: rgba(var(--gsc-text-color), 0.6);
        --gsc-text-color-65: rgba(var(--gsc-text-color), 0.65);
        --gsc-text-color-70: rgba(var(--gsc-text-color), 0.7);
        --gsc-text-color-75: rgba(var(--gsc-text-color), 0.75);
        --gsc-text-color-80: rgba(var(--gsc-text-color), 0.8);
        --gsc-text-color-85: rgba(var(--gsc-text-color), 0.85);
        --gsc-text-color-90: rgba(var(--gsc-text-color), 0.9);
        --gsc-text-color-95: rgba(var(--gsc-text-color), 0.95);
        --gsc-text-color-100: rgba(var(--gsc-text-color), 1);

        --gsc-background-color-0: rgba(var(--gsc-background-color), 0);
        --gsc-background-color-5: rgba(var(--gsc-background-color), 0.05);
        --gsc-background-color-10: rgba(var(--gsc-background-color), 0.1);
        --gsc-background-color-15: rgba(var(--gsc-background-color), 0.15);
        --gsc-background-color-20: rgba(var(--gsc-background-color), 0.2);
        --gsc-background-color-25: rgba(var(--gsc-background-color), 0.25);
        --gsc-background-color-30: rgba(var(--gsc-background-color), 0.3);
        --gsc-background-color-35: rgba(var(--gsc-background-color), 0.35);
        --gsc-background-color-40: rgba(var(--gsc-background-color), 0.4);
        --gsc-background-color-45: rgba(var(--gsc-background-color), 0.45);
        --gsc-background-color-50: rgba(var(--gsc-background-color), 0.5);
        --gsc-background-color-55: rgba(var(--gsc-background-color), 0.55);
        --gsc-background-color-60: rgba(var(--gsc-background-color), 0.6);
        --gsc-background-color-65: rgba(var(--gsc-background-color), 0.65);
        --gsc-background-color-70: rgba(var(--gsc-background-color), 0.7);
        --gsc-background-color-75: rgba(var(--gsc-background-color), 0.75);
        --gsc-background-color-80: rgba(var(--gsc-background-color), 0.8);
        --gsc-background-color-85: rgba(var(--gsc-background-color), 0.85);
        --gsc-background-color-90: rgba(var(--gsc-background-color), 0.9);
        --gsc-background-color-95: rgba(var(--gsc-background-color), 0.95);
